+static inline struct seplist_head** seplist_next_sepentry(void** metaptr)
+{
+	struct seplist_chunk* cur_chunk;
+	struct seplist_head** victim;
+	int chunk_idx;
+
+	cur_chunk = seplist_meta_to_chunk(metaptr);
+	chunk_idx = seplist_meta_to_chunkidx(metaptr) + 1;

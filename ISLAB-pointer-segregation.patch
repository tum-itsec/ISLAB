index 000000000000..56b7adb045cc
--- /dev/null
+++ b/include/linux/islab.h
@@ -0,0 +1,100 @@
+#ifndef _LINUX_ISLAB_H
+#define _LINUX_ISLAB_H
+
+#include <uapi/asm/processor-flags.h>
+
+#ifdef CONFIG_ISLAB
+
+#define islab_palert(fmt, ...) printk("ISLAB Error: %s-%d: " fmt, __FUNCTION__, smp_processor_id(), ##__VA_ARGS__)
+
+#ifdef CONFIG_ISLAB_DEBUG
+
+#define islab_printk(fmt, ...) printk("ISLAB: %s-%d: " fmt, __FUNCTION__, smp_processor_id(), ##__VA_ARGS__)
+
+#else
+
+#define islab_printk(fmt, ...)
+
+#endif
+
+#if defined(CONFIG_ISLAB_VTX)
+#include <linux/vtx.h>
+#elif (defined(CONFIG_ISLAB_SMAP) || defined(CONFIG_SEPLIST_KSMAP))
+#include <linux/ksmap.h>
+#endif
+
+#define __disable_islab_access(s, flags, func)	\
+({						\
+	local_irq_save(flags);			\
+	func(s);				\
+})
+
+#define __enable_islab_access(s, flags, func)	\
+({						\
+	func(s);				\
+	local_irq_restore(flags);		\
+})
+
+#if defined(CONFIG_ISLAB_SMAP)
+
+#if defined(CONFIG_SEPLIST_KSMAP)
+#define disable_islab_access(s, fflags)	\
+({ \
+ 	if (!(s->flags & SLAB_KSMAP)) \
+ 		ksmap_disable_smap(); \
+	(void)fflags; \
+})
+
+#define enable_islab_access(s, fflags)	\
+({ \
+ 	if (!(s->flags & SLAB_KSMAP)) \
+ 		ksmap_enable_smap(); \
+	(void)fflags; \
+})
+#else
+#define disable_islab_access(s, fflags)	\
+({ \
+	ksmap_disable_smap(); \
+	(void)fflags; \
+	(void)s; \
+})
+
+#define enable_islab_access(s, fflags)	\
+({ \
+	ksmap_enable_smap(); \
+	(void)fflags; \
+	(void)s; \
+})
+#endif
+
+#elif defined(CONFIG_ISLAB_VTX)
+
+#define disable_islab_access(s, flags) \
+({ \
+	local_irq_save(flags); \
+	__generic_switch_to_metaview(s); \
+})
+//__disable_islab_access(s, flags, __generic_switch_to_metaview)
+
+#define enable_islab_access(s, flags)	\
+({ \
+	__generic_switch_to_dataview(s); \
+	local_irq_restore(flags); \
+})
+//__enable_islab_access(s, flags, __generic_switch_to_dataview)
+
+#endif /* CONFIG_ISLAB_VTX */
+
+#else /* CONFIG_ISLAB */
+
+#define disable_islab_access(s, flags)	(void)flags
+#define enable_islab_access(s, flags)	(void)flags
+
+#define islab_isolate_pages(h, n)	do {} while (0)
+#define islab_deisolate_pages(h, n)	do {} while (0)
+#define islab_isolate_vaddr(h, n)	do {} while (0)
+#define islab_deisolate_vaddr(h, n)	do {} while (0)
+
+#endif /* !CONFIG_ISLAB */
+
+#endif /* _LINUX_ISLAB_H */

+/**
+ * seplist_cut_position - cut a seplist into two
+ * @seplist: a new seplist to add all removed entries
+ * @head: a seplist with entries
+ * @entry: an entry within head, could be the head itself
+ *	and if so we won't cut the seplist
+ *
+ * This helper moves the initial part of @head, up to and
+ * including @entry, from @head to @seplist. You should
+ * pass on @entry an element you know is on @head. @seplist
+ * should be an empty seplist or a seplist you do not care about
+ * losing its data.
+ *
+ */
+static inline void seplist_cut_position(struct seplist_head *seplist,
+		struct seplist_head *head, struct seplist_head *entry)
+{
+	smapdisable();
+	if (!head->meta || entry == head)
+		INIT_SEPLIST_HEAD(seplist);
+	else
+	{
+		__seplist_cut_position(seplist, (struct seplist_chunk *)head->meta,
+		   (struct seplist_chunk *)entry->meta);
+	}
+	smapenable();
+}
+
+/**
+ * seplist_cut_before - cut a seplist into two, before given entry
+ * @seplist: a new seplist to add all removed entries
+ * @head: a seplist with entries
+ * @entry: an entry within head, could be the head itself
+ *
+ * This helper moves the initial part of @head, up to but
+ * excluding @entry, from @head to @seplist.  You should pass
+ * in @entry an element you know is on @head.  @seplist should
+ * be an empty seplist or a seplist you do not care about losing
+ * its data.
+ * If @entry == @head, all entries on @head are moved to
+ * @seplist.
+ */
+static inline void seplist_cut_before(struct seplist_head *seplist,
+				   struct seplist_head *head,
+				   struct seplist_head *entry)
+{
+	struct seplist_chunk *seplist_c, *head_c, *entry_c;
+
+	seplist_c = (struct seplist_chunk *)seplist->meta;
+	head_c = (struct seplist_chunk *)head->meta;
+	entry_c = (struct seplist_chunk *)entry->meta;
+
+	smapdisable();
+
+	if (!head_c || head_c->next == entry_c) {
+		INIT_SEPLIST_HEAD(seplist);
+		smapenable();
+		return;
+	}
+
+	/* We have to assume an uninitialized seplist */
+	seplist_c = new_seplist_chunk_for_node_init(seplist);
+
+	seplist_c->next = head_c->next;
+	seplist_c->next->prev = seplist_c;
+	seplist_c->prev = entry_c->prev;
+	seplist_c->prev->next = seplist_c;
+	head_c->next = entry_c;
+	entry_c->prev = head_c;
+
+	if(head_c->next == head_c) {
+		head->meta = NULL;
+		__seplist_free_chunk(head_c);
+	}
+	smapenable();
+}

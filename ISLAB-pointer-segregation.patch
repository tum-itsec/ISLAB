+	for (pos = seplist_next_entry(pos, member); \
+	     !seplist_entry_is_head(pos, head, member) && \
+			({ n = seplist_next_entry_unchecked(pos, member); 1; });			\
+	     pos = n)
+
+/**
+ * seplist_for_each_entry_safe_from - iterate over seplist from current point safe against removal
+ * @pos:	the type * to use as a loop cursor.
+ * @n:		another type * to use as temporary storage
+ * @head:	the head for your seplist.
+ * @member:	the name of the seplist_head within the struct.
+ *
+ * Iterate over seplist of given type from current point, safe against
+ * removal of seplist entry.
+ */
+#define seplist_for_each_entry_safe_from(pos, n, head, member)			\
+	for (;!seplist_entry_is_head(pos, head, member) && \
+			({ n = seplist_next_entry_unchecked(pos, member); 1; });			\
+	     pos = n)
+
+/**
+ * seplist_for_each_entry_safe_reverse - iterate backwards over seplist safe against removal
+ * @pos:	the type * to use as a loop cursor.
+ * @n:		another type * to use as temporary storage
+ * @head:	the head for your seplist.
+ * @member:	the name of the seplist_head within the struct.
+ *
+ * Iterate backwards over seplist of given type, safe against removal
+ * of seplist entry.
+ */
+#define seplist_for_each_entry_safe_reverse(pos, n, head, member)		\
+	for (pos = seplist_last_entry(head, typeof(*pos), member);		\
+	     !seplist_entry_is_head(pos, head, member) &&\
+	      ({ n = seplist_prev_entry_unchecked(pos, member); 1; });			\
+	     pos = n)
+
+#define seplist_safe_reset_next(pos, n, member)				\
+	n = seplist_next_entry(pos, member)
+
+

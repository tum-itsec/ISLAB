+/**
+ * seplist_cut_position - cut a seplist into two
+ * @seplist: a new seplist to add all removed entries
+ * @head: a seplist with entries
+ * @entry: an entry within head, could be the head itself
+ *	and if so we won't cut the seplist
+ *
+ * This helper moves the initial part of @head, up to and
+ * including @entry, from @head to @seplist. You should
+ * pass on @entry an element you know is on @head. @seplist
+ * should be an empty seplist or a seplist you do not care about
+ * losing its data.
+ *
+ */
+static inline void seplist_cut_position(struct seplist_head *seplist,
+		struct seplist_head *head, struct seplist_head *entry)
+{	
+	smapdisable();
+	if (!head->meta || entry == head) {
+		INIT_SEPLIST_HEAD(seplist);
+		smapenable();
+		return;
+	}
+	__seplist_cut_position(seplist, head->meta, entry);
+	smapenable();
+}

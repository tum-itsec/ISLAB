+
+	cur_chunk = (struct seplist_chunk *)head->meta;
+
+	smapdisable();
+
+	seplist_check_integrity(head);
+
+	if(!cur_chunk)
+		cur_chunk = new_seplist_chunk_for_node_init(head);
+
+	newchunk = new_seplist_chunk_for_node(new);
+	__seplist_add_chunk(newchunk, cur_chunk, cur_chunk->next);
+	smapenable();
+}

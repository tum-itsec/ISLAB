+/**
+ * seplist_del_rcu - deletes entry from seplist without re-initialization
+ * @entry: the element to delete from the seplist.
+ *
+ * Note: seplist_empty() on entry does not return true after this,
+ * the entry is in an undefined state. It is useful for RCU based
+ * lockfree traversal.
+ *
+ * In particular, it means that we cannot delete the bi-directional linkage
+ * as checks will be performed while walking the entry. Instead the entry
+ * will be marked deleted so that future readers will skip it
+ *
+ * The caller must take whatever precautions are necessary
+ * (such as holding appropriate locks) to avoid racing
+ * with another seplist-mutation primitive, such as seplist_del_rcu()
+ * or seplist_add_rcu(), running on this same seplist.
+ * However, it is perfectly legal to run concurrently with
+ * the _rcu seplist-traversal primitives, such as
+ * seplist_for_each_entry_rcu().
+ *
+ * Note that the caller is permitted to immediately free
+ * the newly deleted entry. Contrary to the standard list interface.
+ * This is due to the fact that this function requires synchronize_rcu()
+ * to properly update the meta datastructure as it does not have access
+ * to an rcu_head to use call_rcu
+ */
+static inline void seplist_del_rcu(struct seplist_head *entry)
+{
+	struct seplist_chunk *cur_chunk;
+	struct seplist_head **next;
+	void** meta = READ_ONCE(entry->meta);
+
+	if(!meta)
+		return;
+
+	smapdisable();
+	
+	seplist_check_integrity(entry);
+
+	cur_chunk = seplist_meta_to_chunk(meta);
+
+	next = seplist_next_sepentry(meta);
+	
+	rcu_assign_pointer(*(void __rcu **)meta, seplist_rcu_delptr(entry));
+	
+	if(sepchunk_unused(cur_chunk)){
+		cur_chunk->next->prev = cur_chunk->prev;
+		rcu_assign_pointer(seplist_chunk_next_rcu(cur_chunk->prev), cur_chunk->next);
+	}
+
+	seplist_rcu_defer_call(__wrapped_seplist_del_deferred_empty_ptr, meta);
+
+	if (seplist_next_sepentry((void **)next) == next) {
+		rcu_assign_pointer(*(void __rcu**)&(*next)->meta, NULL);
+		seplist_rcu_defer_call(__wrapped_seplist_del_deferred_empty_ptr, (void **)next);
+	}
+
+	smapenable();
+}
+
+
+/**
+ * seplist_replace_rcu - replace old entry by new one
+ * @old : the element to be replaced
+ * @new : the new element to insert
+ *
+ * The @old entry will be replaced with the @new entry
+ * Note: @old should not be empty.
+ */
+static inline void seplist_replace_rcu(struct seplist_head *old,
+		struct seplist_head *new)
+{
+	struct seplist_head dummy;
+	void** oldmeta;
+	//printk("replace_rcu %llx with %llx\n", (u64)old, (u64)new);
+	smapdisable();
+	seplist_check_integrity(old);
+
+	/* Relink the old entry to a position where checks will not fail */
+	seplist_add_rcu(seplist_rcu_delptr(&dummy), old);
+	smapdisable(); // TODO seplist_add_rcu returns with smap enabled!
+	oldmeta = READ_ONCE(old->meta);
+
+   *dummy.meta = seplist_rcu_delptr(old);
+   rcu_assign_pointer(*(void __rcu **)&old->meta, dummy.meta);
+
+	new->meta = oldmeta;
+	rcu_assign_pointer(*(void __rcu **)oldmeta, (void*)new);
+	seplist_rcu_defer_call(__wrapped_seplist_del_deferred_empty_ptr, (void*)dummy.meta);
+	smapenable();
+}

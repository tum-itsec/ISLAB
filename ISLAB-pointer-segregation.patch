+static inline void __seplist_add_chunk_rcu(struct seplist_chunk* new,
+		struct seplist_chunk* prev,
+		struct seplist_chunk* next)
+{
+	new->next = next;
+	new->prev = prev;
+	rcu_assign_pointer(seplist_chunk_next_rcu(prev), new);
+	next->prev = new;
+}
+
+static inline void __seplist_deferred_del_chunk(void *_chunk)
+{
+	struct seplist_chunk *chunk = _chunk;
+	smapdisable();
+	__seplist_free_chunk(chunk);
+	smapenable();
+}

+/**
+ * Special function that will delete all entries between head and first
+ */
+static inline void seplist_del_many(struct seplist_head *head,
+				   struct seplist_head *first)
+{
+	size_t delcnt;
+	struct seplist_chunk *head_c, *first_c, *to_del, *tmp;
+	struct seplist_head **head_m, **first_m;
+
+
+	head_m = (struct seplist_head **)head->meta;
+	first_m = (struct seplist_head **)first->meta;
+
+	if(!head_m)
+		return;
+
+	smapdisable();
+
+	head_c = seplist_meta_to_chunk(head_m);
+	first_c = seplist_meta_to_chunk(first_m);
+
+	delcnt = SEPLIST_CHUNK_ENTRIES - ((void**)head_m - head_c->obj);
+	memset((void*)head_m, 0, sizeof(void*) * delcnt);
+
+	delcnt = 1 + ((void**)first_m - first_c->obj);
+	memset(head_c->obj, 0, sizeof(void*) * delcnt);
+
+	to_del = head_c->next;
+	first_c->prev = head_c;
+	WRITE_ONCE(head_c->next, first_c);
+
+	while(to_del != first_c)
+	{
+		tmp = to_del->next;
+		__seplist_free_chunk(to_del);
+		to_del = tmp;
+	}
+
+	/* Delete empty heads to prevent memory leaks*/
+	if(seplist_next_sepentry((void **)head_m) == head_m)
+	{
+		head->meta = NULL;
+		__seplist_free_chunk(head_c);
+	}
+	smapenable();
+}

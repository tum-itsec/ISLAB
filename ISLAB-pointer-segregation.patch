diff --git a/kernel/seplist.c b/kernel/seplist.c
new file mode 100644
index 000000000000..126f7e6415cd
--- /dev/null
+++ b/kernel/seplist.c
@@ -0,0 +1,317 @@
+
+#include <linux/list.h>
+#include <linux/seplist.h>
+#include <linux/rcuseplist.h>
+
+#include <linux/slab.h>
+#include <linux/gfp.h>
+#include <linux/memblock.h>
+
+/**
+ * --- Early list allocator ---
+ *  The early list allocator uses memblock to alloc/free nodes and later
+ *  migrates them to a pagelist allocated page and later to kmem_cache
+ *
+ *  To faciliate this, it consists of two parts:
+ *   - memblock calls to allocate objects
+ *   - simple linked list to store list of allocated objects
+ *
+ *  Migration routines
+ * **/
+
+#define STATIC_INIT_LIST_ENTRIES 0x1000
+
+struct early_meta {
+#ifdef CONFIG_SEPLIST_MAGAZINE
+	struct seplist_chunk chunk __attribute__((aligned (sizeof(struct seplist_chunk))));
+#else
+	struct seplist_chunk chunk;
+#endif
+	struct list_head_orig list;
+};
+
+static __initdata LIST_HEAD_ORIG(early_blocks);
+
+static __initdata struct
+{
+	struct early_meta memory_area[STATIC_INIT_LIST_ENTRIES];
+	size_t fill;
+} prememblock_memory = {
+	.fill = 0,
+};
+
+struct seplist_head *wrapped_seplist_next_unchecked(const struct seplist_head *pos)
+{
+	struct seplist_head *ret;
+	smapdisable();
+	ret = _seplist_next_unchecked(pos);
+	smapenable();
+	return ret;
+}
+
+struct seplist_head *wrapped_seplist_prev_unchecked(const struct seplist_head *pos)
+{
+	struct seplist_head *ret;
+	smapdisable();
+	ret = _seplist_prev_unchecked(pos);
+	smapenable();
+	return ret;
+}
+
+#ifdef CONFIG_SEPLIST_MAGAZINE
+
+struct seplist_head *wrapped_seplist_next_rcu(const struct seplist_head *pos)
+{
+	void **___meta;
+	struct seplist_head *ret = NULL;
+	struct seplist_head const *___ptr = (pos);
+	smapdisable();
+	___meta = READ_ONCE(___ptr->meta);
+
+	seplist_check_integrity(pos);
+	if (___meta) {
+		ret = seplist_next_from_meta(___meta);
+	} else {
+		ret = (struct seplist_head *)___ptr;
+	}
+	smapenable();
+	return ret;
+}
+
+struct seplist_head *wrapped_seplist_tail_rcu(const struct seplist_head *pos)
+{
+	struct seplist_head *ret = NULL;
+	struct seplist_head const *___ptr = (pos);
+	void **___meta = READ_ONCE(___ptr->meta);
+	smapdisable();
+	seplist_check_integrity(pos);
+	if (___meta) {
+		ret = seplist_prev_from_meta(___meta);
+	} else {
+		ret = (struct seplist_head *)___ptr;
+	}
+	smapenable();
+	return ret;
+}
+
+#elif defined(CONFIG_SEPLIST_SINGLE)
+
+struct seplist_head *wrapped_seplist_next_rcu(const struct seplist_head *pos)
+{
+	struct seplist_head *ret = NULL;
+	struct seplist_head const *___ptr = (pos);
+	struct seplist_chunk *___meta = (struct seplist_chunk *)READ_ONCE(___ptr->meta);
+	smapdisable();
+	seplist_check_integrity(pos);
+	if (___meta) {
+		ret = READ_ONCE(*(struct seplist_head __rcu **)&(READ_ONCE(___meta->next)->obj));
+	} else {
+		ret = (struct seplist_head *)___ptr;
+	}
+	smapenable();
+	return ret;
+}
+
+struct seplist_head *wrapped_seplist_tail_rcu(const struct seplist_head *pos)
+{
+	struct seplist_head *ret = NULL;
+	struct seplist_head const *___ptr = (pos);
+	struct seplist_chunk *___meta = (struct seplist_chunk *)READ_ONCE(___ptr->meta);
+	smapdisable();
+	seplist_check_integrity(pos);
+	if (___meta) {
+		ret = READ_ONCE(*(struct seplist_head __rcu **)&(READ_ONCE(___meta->prev)->obj));
+	} else {
+		ret = (struct seplist_head *)___ptr;
+	}
+	smapenable();
+	return ret;
+}
+
+struct seplist_head *wrapped_seplist_next_rcu_unchecked(const struct seplist_head *pos)
+{
+	struct seplist_head *ret;
+	smapdisable();
+	ret = _seplist_next_rcu_unchecked(pos);
+	smapenable();
+	return ret;
+}
+
+struct seplist_head *wrapped_seplist_tail_rcu_unchecked(const struct seplist_head *pos)
+{
+	struct seplist_head *ret;
+	smapdisable();
+	ret = _seplist_tail_rcu_unchecked(pos);
+	smapenable();
+	return ret;
+}
+
+#endif
+
+static struct early_meta * __init prememblock_alloc(void) {
+	printk("allocating prememblock number %d\n", (int)prememblock_memory.fill);
+
+#ifdef CONFIG_SEPLIST_MAGAZINE
+	/* HACKY!!! */
+	if (unlikely(!prememblock_memory.fill)) {
+		spin_lock_init(&seplist_rcu_deferred_del_spinlock);
+	}
+#endif
+	return &prememblock_memory.memory_area[prememblock_memory.fill++];
+}
+
+static struct seplist_chunk * __init early_new_seplist_chunk(void)
+{
+	struct early_meta *_new = prememblock_alloc();
+
+	memset(&_new->chunk, 0, sizeof(struct seplist_chunk));
+
+	list_add_orig(&_new->list, &early_blocks);
+
+	return &_new->chunk;
+}
+
+
+static void __init __early_seplist_free_chunk(struct seplist_chunk *chunk)
+{
+	struct early_meta *_meta = container_of(chunk, struct early_meta, chunk);
+	list_del_orig(&_meta->list);
+}
+
+
+static void __init migrate_chunkdata(struct seplist_chunk *old, struct seplist_chunk *new)
+{
+#ifdef CONFIG_SEPLIST_SINGLE
+	new->obj = old->obj;
+	((struct seplist_head *)new->obj)->meta = (void**)new;
+#elif defined(CONFIG_SEPLIST_MAGAZINE)
+	size_t i;
+	for(i = 0; i < SEPLIST_CHUNK_ENTRIES; i++)
+	{
+		new->obj[i] = old->obj[i];
+		if(new->obj[i])
+		{
+			((struct seplist_head *)new->obj[i])->meta = &new->obj[i];
+		}
+	}
+#endif
+}
+
+
+/**
+ * Note, interrupts and SMP is enabled, so we do not need to consider races
+ * We assume that the amount of used lists in this stage is low enough to
+ * fit all metadata on the initial slab, i.e. no new slabs are required
+ * and so no new page allocations are performed
+ */
+static void __init migrate_chunk_to_kmem_cache(struct early_meta* chunk)
+{
+	struct seplist_chunk *_new;
+	_new = new_seplist_chunk();
+
+	smapdisable();
+
+	migrate_chunkdata(&chunk->chunk, _new);
+
+	/* If the chunk was not the only entry */
+	if (chunk->chunk.next != &chunk->chunk)
+	{
+		__seplist_unlink_chunk(&chunk->chunk);
+		__seplist_add_chunk(_new, chunk->chunk.prev, chunk->chunk.next);
+	}
+	else
+	{
+		_new->next = _new;
+		_new->prev = _new;
+	}
+
+	__early_seplist_free_chunk(&chunk->chunk);
+	
+	smapenable();
+}
+
+/**
+ * Note, interrupts and SMP is enabled, so we do not need to consider races
+ * We assume that the amount of used lists in this stage is low enough to
+ * fit all metadata on the initial slab, i.e. no new slabs are required
+ * and so no new page allocations are performed
+ */
+static void __init migrate_early_list_allocator_to_kmem_cache(void)
+{
+	LIST_HEAD_ORIG(tmp_early_chunks);
+	list_splice_init_orig(&early_blocks, &tmp_early_chunks);
+
+	/* Allocate enough pages */
+	while(!list_empty_orig(&tmp_early_chunks))
+	{
+		migrate_chunk_to_kmem_cache(
+			list_first_entry_orig(&tmp_early_chunks, struct early_meta, list));
+	}
+}
+
+
+#ifdef CONFIG_SEPLIST_MAGAZINE
+spinlock_t seplist_rcu_deferred_del_spinlock;
+EXPORT_SYMBOL(seplist_rcu_deferred_del_spinlock);
+#endif
+
+struct seplist_chunk __ref *new_seplist_chunk(void)
+{
+	if(likely(kmem_seplist_chunk_cache))
+		return kmem_cache_zalloc(kmem_seplist_chunk_cache, GFP_NOWAIT);
+	else
+		return early_new_seplist_chunk();
+}
+EXPORT_SYMBOL(new_seplist_chunk);
+
+void __ref __seplist_free_chunk(struct seplist_chunk *chunk)
+{
+	if(likely(kmem_seplist_chunk_cache))
+		kmem_cache_free(kmem_seplist_chunk_cache, chunk);
+	else
+		__early_seplist_free_chunk(chunk);
+}
+EXPORT_SYMBOL(__seplist_free_chunk);
+
+void seplist_rcu_do_call(struct rcu_head *head)
+{
+	struct seplist_rcu_info* ptr = container_of(head, struct seplist_rcu_info, rcu);
+	ptr->f(ptr->data);
+	kmem_cache_free(kmem_seplist_rcustruct_cache, ptr);
+}
+EXPORT_SYMBOL(seplist_rcu_do_call);
+
+struct seplist_rcu_info *new_seplist_rcu_info(void)
+{
+	return kmem_cache_alloc(kmem_seplist_rcustruct_cache, GFP_NOWAIT);
+}
+EXPORT_SYMBOL(new_seplist_rcu_info);
+
+void __init create_seplist_chunk_cache(slab_flags_t flags)
+{
+	if(!kmem_seplist_rcustruct_cache)
+		kmem_seplist_rcustruct_cache = kmem_cache_create("seplist_rcustruct",
+				sizeof(struct seplist_rcu_info),
+				0, flags, NULL);
+	
+	if(!kmem_seplist_chunk_cache)
+#ifdef CONFIG_SEPLIST_MAGAZINE

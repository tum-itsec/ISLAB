+/*
+ *  *  * Use a structure to store
+ *   *   */
+struct altp2m_vcpu {
+	int nr;
+	int status;
+	struct ve_info_area *vea;
+};
+
+struct altp2m_vcpu_info {
+	unsigned int num_vcpus;
+	struct altp2m_vcpu vcpus[CONFIG_NR_CPUS];
+};
+
+typedef enum {
+	ALTP2M_VCPU_STATUS_DOWN,
+	ALTP2M_VCPU_STATUS_UP
+} altp2m_vcpu_status_t;
+
+static struct altp2m_vcpu_info altp2m_vcpu_info = { .num_vcpus = CONFIG_NR_CPUS };
+
+static struct altp2m_vcpu *altp2m_vcpu_ptr(unsigned int cpu)
+{
+	if (cpu >= altp2m_vcpu_info.num_vcpus)
+		return NULL;
+
+	return &altp2m_vcpu_info.vcpus[cpu];
+}
+
+struct ve_info_area *altp2m_vcpu_vea(unsigned int cpu)
+{
+	struct altp2m_vcpu *vcpu = altp2m_vcpu_ptr(cpu);
+
+	return (vcpu ? vcpu->vea : NULL);
+}
+
+static void * altp2m_alloc_node(size_t size, int nid)
+{
+	return kzalloc_node(size, GFP_KERNEL, nid);
+}
+
+static void * altp2m_alloc_cpu(size_t size, unsigned int cpu)
+{
+	return altp2m_alloc_node(size, cpu_to_node(cpu));
+}
+
+static int altp2m_alloc_vcpu_vea(void)
+{
+	unsigned int cpu;
+	struct altp2m_vcpu *vcpu;
+
+	for_each_possible_cpu(cpu) {
+#if 0
+		vcpu = altp2m_vcpu_ptr(cpu);
+
+		vcpu->vea = altp2m_alloc_cpu(PAGE_SIZE, cpu);
+		if (!vcpu->vea)
+			return -ENOMEM;
+#endif
+		vcpu = &altp2m_vcpu_info.vcpus[cpu];
+
+		vcpu->status = ALTP2M_VCPU_STATUS_DOWN;
+		vcpu->nr = cpu;
+	}
+
+	return 0;
+}
+
+static int altp2m_alloc_vcpus(void)
+{
+	unsigned int cpus, size;
+
+	cpus = num_possible_cpus();
+	size = sizeof(struct altp2m_vcpu) * cpus;
+
+#if 0
+	altp2m_vcpu_info.vcpus = altp2m_alloc_node(size, NUMA_NO_NODE);
+	if (!altp2m_vcpu_info.vcpus)
+		return -ENOMEM;
+#endif
+
+	altp2m_vcpu_info.num_vcpus = cpus;
+
+	/* Allocate virtualization exception area */
+	if (altp2m_alloc_vcpu_vea())
+		return -ENOMEM;
+
+	return 0;
+}
+
+static void altp2m_init_vcpu(void *info)
+{
+	unsigned int cpu = smp_processor_id();
+	struct altp2m_vcpu *vcpu = altp2m_vcpu_ptr(cpu);
+	int rc;                                                             
+	xen_pfn_t gfn;
+	
+	if (!vcpu)
+		return;
+   
+	vcpu->vea = (struct ve_info_area *)page_to_virt(alloc_page(GFP_KERNEL | __GFP_ZERO));
+
+	gfn = __pa(vcpu->vea) >> PAGE_SHIFT;                      
+
+	rc = altp2m_set_vcpu_enable_notify(vcpu->nr, gfn);                  
+
+	if (rc) {                                                           
+		printk("Error setting vea_struct in vCPU %d: %d", vcpu->nr, rc);
+		return;                                                         
+	}                                                                   
+
+	vcpu->status = ALTP2M_VCPU_STATUS_UP;
+
+	printk("altp2m: Initialized CPU %u\n", cpu);
+}
+
+void altp2m_init_vcpus_except_self(void)
+{
+	struct cpumask cpumask_except_self;
+	
+	cpumask_copy(&cpumask_except_self, cpu_online_mask);
+
+	cpumask_clear_cpu(smp_processor_id(), &cpumask_except_self);
+
+	on_each_cpu_mask(&cpumask_except_self, altp2m_init_vcpu, NULL, 1);
+}

+		*listmeta = (void*)head;
+		head->meta = listmeta;
+		smapenable();
+		return;
+	}
+
+	__seplist_splice(seplist_prev_metaspot(list->meta), seplist_prev_metaspot(head->meta));
+	/* Delete previous head */
+	*list->meta = (void*)NULL;
+
+	chunk = seplist_meta_to_chunk(listmeta);
+	*listmeta = (void*)NULL;
+
+	/* Check if chunk empty now and clean up if necessary */
+	if(sepchunk_empty(chunk))
+		__seplist_del_chunk(chunk);
+	smapenable();

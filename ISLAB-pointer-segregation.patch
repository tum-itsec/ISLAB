diff --git a/mm/vtx.c b/mm/vtx.c
new file mode 100644
index 000000000000..f380b4e17d25
--- /dev/null
+++ b/mm/vtx.c
@@ -0,0 +1,203 @@
+#include <linux/mm.h>
+#include <linux/hugetlb.h>
+#include <linux/irqflags.h>
+#include <linux/vtx.h>
+
+#include <asm/pgtable.h>
+#include <asm/uaccess.h>
+
+#include <asm/set_memory.h>
+#include <asm/pgalloc.h>
+
+#ifdef CONFIG_ISLAB_VTX
+
+/*
+ * We move these variables from the (old) mm/metaslab.c file here as the
+ * views may change depending on the underlying isolation mechanism.
+ */
+
+uint16_t dataview, metaview;
+EXPORT_SYMBOL(dataview);
+EXPORT_SYMBOL(metaview);
+
+/*
+ * To make each CPU interpret the vmfunc instruction correctly, we need
+ * at least one hypercall that instructs Xen to actually create pdomain
+ * views. We allocate a single page here and isolate it.
+ */
+struct page *__dummy_page;
+
+/*
+ * Generic Intel VT-x isolation primitives for switching domains. These
+ * functions are only necessary if CONFIG_SMAP is disabled.
+ */
+
+void __generic_switch_to_metaview(const struct kmem_cache *s)
+{
+	int rc;
+
+#ifdef CONFIG_SLUB_HARDENED_FREELIST_METASLAB_SELECTIVE
+	if (!(s->flags & SLAB_ISOLATE_FP))
+		return;
+#endif
+	rc = vmfunc(metaview);
+	if (rc) {
+		islab_palert("Could not switch to metaview (%u): %d", metaview, rc);
+		BUG_ON(true);
+	}
+}
+
+void __generic_switch_to_dataview(const struct kmem_cache *s)
+{
+	int rc;
+
+#ifdef CONFIG_SLUB_HARDENED_FREELIST_METASLAB_SELECTIVE
+	if (!(s->flags & SLAB_ISOLATE_FP))
+		return;
+#endif
+	rc = vmfunc(dataview);
+	if (rc) {
+		islab_palert("Could not switch to dataview (%u): %d", dataview, rc);
+		BUG_ON(true);
+	}
+}
+
+static __always_inline int islab_altp2m_set_access(struct page *page,
+		uint16_t altp2m_id, xenmem_access_t access)
+{
+	int rc;
+	xen_pfn_t gfn = page_to_pfn(page);
+
+	rc = altp2m_set_mem_access(altp2m_id, gfn, access);
+	if (rc) {
+		BUG_ON(true);
+	}
+
+#ifdef CONFIG_ISLAB_DEBUG
+	/*
+	 * We set the SVE bit according to the permissions set for this PFN.
+	 * If the permission enumerator is bigger than XENMEM_access_n, then
+	 * we grant the PFN access permissions, i.e. we lift the isolation.
+	 *
+	 * Lifting isolation restrictions means, we want to suppress #VE for
+	 * this PFN, and vice versa.
+	 */
+	rc = altp2m_set_suppress_ve(altp2m_id, gfn, (access > XENMEM_access_n));
+	if (rc) {
+		islab_palert("Error calling altp2m_set_suppress_ve: %d", rc);
+		// BUG_ON(true);
+	}
+#endif
+
+	return rc;
+}
+
+static void islab_isolate_single_page(struct page *page)
+{
+	islab_altp2m_set_access(page, dataview, XENMEM_access_n);
+}
+
+static void islab_deisolate_single_page(struct page *page)
+{
+	islab_altp2m_set_access(page, dataview, XENMEM_access_rwx);
+}
+
+void islab_isolate_pages(struct page *head, unsigned long num_pages)
+{
+	unsigned long offset;
+
+	for (offset = 0; offset < num_pages; offset++) {
+		islab_isolate_single_page(head + offset);
+	}
+}
+
+void islab_deisolate_pages(struct page *head, unsigned long num_pages)
+{
+	unsigned long offset;
+
+	for (offset = 0; offset < num_pages; offset++) {
+		islab_deisolate_single_page(head + offset);
+	}
+}
+
+void islab_isolate_vaddr(void *vaddr, unsigned long num_pages)
+{
+	unsigned long offset;
+
+	for (offset = 0; offset < num_pages; offset++) {
+		struct page *page = vmalloc_to_page(vaddr + (offset * PAGE_SIZE));
+
+		islab_isolate_single_page(page);
+	}
+}
+
+void islab_deisolate_vaddr(void *vaddr, unsigned long num_pages)
+{
+	unsigned long offset;
+
+	for (offset = 0; offset < num_pages; offset++) {
+		struct page *page = vmalloc_to_page(vaddr + (offset * PAGE_SIZE));
+
+		islab_deisolate_single_page(page);
+	}
+}
+
+static void __init islab_init_cpu(void *info)
+{
+	int rc;
+
+	/*
+	 * Perform bogus operations that instruct Xen to actually create the
+	 * different pdomains. Without these two calls, the CPU will not see
+	 * the vmfunc instruction but an invalid opcode.
+	 */
+	islab_altp2m_set_access(__dummy_page, dataview, XENMEM_access_rwx);
+	islab_altp2m_set_access(__dummy_page, metaview, XENMEM_access_rwx);
+
+	rc = vmfunc(dataview);
+	if (rc) {
+		islab_palert("Error switching to dataview: %d", rc);
+		// BUG_ON(true);
+	} else {
+		printk("ISLAB: Initialized CPU %d", smp_processor_id());
+	}
+}
+
+void __init islab_init_late(void)
+{
+	int curr_cpu = smp_processor_id();
+	struct cpumask uninitialized_cpus;
+
+	cpumask_copy(&uninitialized_cpus, cpu_online_mask);
+	cpumask_clear_cpu(curr_cpu, &uninitialized_cpus);
+
+	on_each_cpu_mask(&uninitialized_cpus, islab_init_cpu, NULL, 1);
+}
+
+void __init islab_init(void) {
+	int rc;
+
+	rc = altp2m_create_view(XENMEM_access_default, &dataview);
+	if (rc) {
+		islab_palert("Error creating the dataview: %d", rc);
+		BUG_ON(true);
+	}
+	
+	rc = altp2m_create_view(XENMEM_access_default, &metaview);
+	if (rc) {
+		islab_palert("Error creating the metaview: %d", rc);
+		BUG_ON(true);
+	}
+
+	printk("Created altp2m dataview: %u", dataview);
+	printk("Created altp2m metaview: %u", metaview);
+	
+	printk("Finished creating views");
+
+	/* See comment at variable declaration */
+	__dummy_page = alloc_page(GFP_KERNEL);
+	
+	islab_init_cpu(NULL);
+}
+
+#endif /* CONFIG_ISLAB_VTX */

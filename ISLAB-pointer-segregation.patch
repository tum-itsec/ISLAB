+// Module metadata
+MODULE_AUTHOR("Simon Schnueckel");
+MODULE_DESCRIPTION("Ksmap Allocator test module");
+MODULE_LICENSE("GPL");
+
+#define DEBUG
+
+#ifdef DEBUG
+#define test_printk(fmt, ...) printk(" " #fmt, ##__VA_ARGS__)
+#else
+#define test_printk(fmt, ...) (void)fmt
+#endif
+
+static int ksmap_test(void)
+{
+	struct page *firstalloc, *secondalloc, *thirdalloc, *forthalloc;
+	void *addr1, *addr2, *addr3, *addr4;
+	uint64_t *access_test;
+
+	struct page *alloc3, *alloc4, *alloc5, *alloc6, *alloc7, *alloc8, *alloc9;
+
+	test_printk("[TEST MODULE]: Test 0 -> Standard allocations");
+	firstalloc = get_isolated_pages(3);
+	addr1 = page_address(firstalloc);
+	test_printk("[TEST MODULE]: Order 3 Buddy received at: %lx", (unsigned long)addr1);
+
+	secondalloc = get_isolated_pages(2);
+	addr2 = page_address(secondalloc);
+	test_printk("[TEST MODULE]: Order 2 Buddy received at: %lx", (unsigned long)addr2);
+
+	thirdalloc = get_isolated_pages(0);
+	addr3 = page_address(thirdalloc);
+	test_printk("[TEST MODULE]: Order 0 Buddy received at: %lx", (unsigned long)addr3);
+
+	forthalloc = get_isolated_pages(1);
+	addr4 = page_address(forthalloc);
+	test_printk("[TEST MODULE]: Order 1 Buddy received at: %lx", (unsigned long)addr4);
+
+	test_printk("[TEST MODULE]: Test 1 -> Splits and merges");
+	// Test alloc and free different sizes
+	alloc3 = get_isolated_pages(2);
+	alloc4 = get_isolated_pages(2);
+	free_isolated_pages(alloc4, 2);
+	free_isolated_pages(alloc3, 2);
+	alloc3 = get_isolated_pages(2);
+	
+	test_printk("[TEST MODULE]: Test 2 -> Regular access and more allocations");
+	// Test regular allowed access
+	access_test = page_address(alloc3);
+	ksmap_disable_smap();
+	printk("I should not crash!");
+	*access_test = 0x69;
+	ksmap_enable_smap();
+
+	// More alloc and free tests
+	alloc4 = get_isolated_pages(2);
+	free_isolated_pages(alloc4, 2);
+	free_isolated_pages(alloc3, 2);
+	alloc5 = get_isolated_pages(4);
+
+	test_printk("[TEST MODULE]: Test 3 -> Irregular access crash test");
+	// Access test with Crash!
+	access_test = page_address(alloc5);
+	// printk("I should crash!");
+	// *access_test = 0x69; // Crashes
+
+	test_printk("[TEST MODULE]: Test 4 -> Free everything");
+	// More freeing
+	free_isolated_pages(secondalloc, 2);
+	free_isolated_pages(firstalloc, 3);
+	free_isolated_pages(thirdalloc, 3);
+	free_isolated_pages(forthalloc, 3);
+	free_isolated_pages(alloc5, 4);
+	// Everything free again now!
+
+	test_printk("[TEST MODULE]: Test 5 -> Large Memory and releasing mem to buddy");
+	// Tests on out of memory scenario
+	alloc3 = get_isolated_pages(8); // its buddy is always in use, islab holds it
+	alloc4 = get_isolated_pages(8); // gets new page from buddy
+	alloc5 = get_isolated_pages(8); 
+	alloc6 = get_isolated_pages(8); // gets new page from buddy
+	alloc7 = get_isolated_pages(8);
+	alloc8 = get_isolated_pages(8); // gets new page from buddy
+	alloc9 = get_isolated_pages(8);
+	free_isolated_pages(alloc4, 8); // no giving back pages to buddy
+	free_isolated_pages(alloc3, 8); // no giving back pages to buddy -> islab holds buddy
+	free_isolated_pages(alloc5, 8); // should give back pages to buddy
+	free_isolated_pages(alloc7, 8); // no giving back pages to buddy
+	free_isolated_pages(alloc6, 8); // should give back pages to buddy
+	free_isolated_pages(alloc8, 8); // no giving back pages to buddy
+	free_isolated_pages(alloc9, 8); // should give back pages to buddy
+
+	alloc3 = get_isolated_pages(2);
+	alloc4 = get_isolated_pages(2);
+	free_isolated_pages(alloc4, 2);
+	free_isolated_pages(alloc3, 2);
+	alloc3 = get_isolated_pages(4);
+	free_isolated_pages(alloc3, 4);
+
+	test_printk("[TEST MODULE]: FINISHED!");
+	return 0;
+}
+
+#include <linux/seplist.h>
+struct testest {
+	int check;
+	int doublecheck;
+};
+static int cache_ksmap_test(void)
+{
+	struct kmem_cache *kmem_seplist_chunk_cache = NULL;
+	struct testest *test = NULL;
+	test_printk("[TEST MODULE]: Start");
+	kmem_seplist_chunk_cache = kmem_cache_create("seplist_chunks",
+											  sizeof(struct testest),
+											  0, SLAB_HWCACHE_ALIGN, NULL);
+	test_printk("[TEST MODULE]: Cache created");
+	test = kmem_cache_alloc(kmem_seplist_chunk_cache, GFP_NOWAIT);
+	test_printk("[TEST MODULE]: Allocated");
+	// should crash with smap error!
+	test->check = 42;
+	test_printk("[TEST MODULE]: Accessed");
+	test_printk("[TEST MODULE]: FINISHED");
+	return 0;
+}
+
+static __init int ksmap_test_init(void)
+{
+	return cache_ksmap_test();
+}
+
+static __exit void ksmap_test_exit(void)
+{
+	test_printk("Exiting");
+}
+
+module_init(ksmap_test_init);
+module_exit(ksmap_test_exit);

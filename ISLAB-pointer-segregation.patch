+/**
+ * seplist_add_tail - add a new entry
+ * @new: new entry to be added
+ * @head: list head to add it before
+ *
+ * Insert a new entry before the specified head.
+ * This is useful for implementing queues.
+ */
+static inline void seplist_add_tail(struct seplist_head *new, struct seplist_head *head)
+{
+	struct seplist_chunk* cur_chunk, *newchunk;
+
+	cur_chunk = (struct seplist_chunk *)head->meta;
+
+	smapdisable();
+
+	seplist_check_integrity(head);
+
+	if(!cur_chunk)
+		cur_chunk = new_seplist_chunk_for_node_init(head);
+
+	newchunk = new_seplist_chunk_for_node(new);
+
+	__seplist_add_chunk(newchunk, cur_chunk->prev, cur_chunk);
+	smapenable();
+}
+
+/**
+ * Adds a new entry to the tail of the seplist in a lockless way, i.e.
+ * multiple CPUs are allowed to call this function concurrently.
+ *
+ * Beware: it is necessary to prevent any other modifications of the
+ *         existing seplist until all changes are completed, in other words
+ *         concurrent seplist_add_tail_lockless() calls should be protected
+ *         with a read lock, where write lock acts as a barrier which
+ *         makes sure all seplist_add_tail_lockless() calls are fully
+ *         completed.
+ *
+ *        Also an element can be locklessly added to the seplist only in one
+ *        direction i.e. either to the tail either to the head, otherwise
+ *        concurrent access will corrupt the seplist.
+ *
+ * Returns %false if element has been already added to the seplist, %true
+ * otherwise.
+ */
+static inline bool seplist_add_tail_lockless(struct seplist_head *new,
+					  struct seplist_head *head)
+{
+	struct seplist_chunk *newchunk, *headchunk, *tmp;
+	struct seplist_chunk*prev;
+
+	smapdisable();
+
+	headchunk = (struct seplist_chunk *)READ_ONCE(head->meta);
+	if(!headchunk)
+	{
+		tmp = new_seplist_chunk_init();
+		tmp->obj = head;
+		/*
+		 * This is simple 'head->meta = tmp' operation, but cmpxchg()
+		 * is used in order to detect that the head has been just
+		 * allocated from another CPU: the winner observes
+		 * head->meta == NULL.
+		 */
+		if (cmpxchg(&head->meta, NULL, (void*)tmp) != NULL)
+		{
+			/* On fail, read the headchunk and free previously allocated chunk */
+			__seplist_free_chunk(tmp);
+			headchunk = (struct seplist_chunk *)READ_ONCE(head->meta);
+		}
+		else
+			headchunk = tmp;
+	}
+
+	/* Repeat the same procedure for inserted obj, but abort if another CPU succeeded */
+	newchunk = (struct seplist_chunk *)READ_ONCE(new->meta);
+	if(!newchunk)
+	{
+		tmp = new_seplist_chunk_init();
+		tmp->obj = new;
+		/*
+		 * This is simple 'new->meta = tmp' operation, but cmpxchg()
+		 * is used in order to detect that the head has been just
+		 * allocated from another CPU: the winner observes
+		 * new->meta == NULL.
+		 */
+		if (cmpxchg(&new->meta, NULL, (void*)tmp) != NULL)
+		{
+			/* On fail, free previously allocated chunk and return as another CPU
+			 * is dealing with this object */
+			__seplist_free_chunk(tmp);
+			smapenable();
+			return false;
+		}
+		newchunk = tmp;
+	}
+
+
+	/*
+	 * This is simple 'newchunk->next = headchunk' operation, but cmpxchg()
+	 * is used in order to detect that same element has been just
+	 * added to the seplist from another CPU: the winner observes
+	 * newchunk->next == newchunk.
+	 */
+	if (cmpxchg(&newchunk->next, newchunk, headchunk) != newchunk) {
+		smapenable();
+		return false;
+	}
+
+	/*
+	 * Initially ->next of a new element must be updated with the head
+	 * (we are inserting to the tail) and only then pointers are atomically
+	 * exchanged.  XCHG guarantees memory ordering, thus ->next should be
+	 * updated before pointers are actually swapped and pointers are
+	 * swapped before prev->next is updated.
+	 */
+
+	prev = xchg(&headchunk->prev, newchunk);
+
+	/*
+	 * It is safe to modify prev->next and new->prev, because a new element
+	 * is added only to the tail and new->next is updated before XCHG.
+	 */
+
+	prev->next = newchunk;
+	newchunk->prev = prev;
+	
+	smapenable();
+	return true;
+}

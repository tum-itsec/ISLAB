+/**
+ * seplist_entry - get the struct for this entry
+ * @ptr:	the &struct seplist_head pointer.
+ * @type:	the type of the struct this is embedded in.
+ * @member:	the name of the seplist_head within the struct.
+ */
+#define seplist_entry(ptr, type, member) \
+	container_of(ptr, type, member)
+
+
+/**
+ * seplist_first_entry - get the first element from a list
+ * @ptr:	the seplist head to take the element from.
+ * @type:	the type of the struct this is embedded in.
+ * @member:	the name of the seplist_head within the struct.
+ *
+ * Note, that list is expected to be not empty.
+ */
+#define seplist_first_entry(ptr, type, member) \
+	seplist_entry(seplist_next((ptr)), type, member)
+
+/**
+ * seplist_last_entry - get the last element from a seplist
+ * @ptr:	the seplist head to take the element from.
+ * @type:	the type of the struct this is embedded in.
+ * @member:	the name of the seplist_head within the struct.
+ *
+ * Note, that seplist is expected to be not empty.
+ */
+#define seplist_last_entry(ptr, type, member) \
+	seplist_entry(seplist_prev((ptr)), type, member)
+
+
+/**
+ * seplist_first_entry_or_null - get the first element from a seplist
+ * @ptr:	the seplist head to take the element from.
+ * @type:	the type of the struct this is embedded in.
+ * @member:	the name of the seplist_head within the struct.
+ *
+ * Note that if the seplist is empty, it returns NULL.
+ */
+#define seplist_first_entry_or_null(ptr, type, member) ({ \
+	struct seplist_head *head__ = (ptr); \
+	struct seplist_head *pos__ = seplist_next(head__); \
+	pos__ != head__ ? seplist_entry(pos__, type, member) : NULL; \
+})

+/**
+ * seplist_add - add a new entry
+ * @new: new entry to be added
+ * @head: list head to add it after
+ *
+ * Insert a new entry after the specified head.
+ * This is good for implementing stacks.
+ */
+static inline void seplist_add(struct seplist_head *new, struct seplist_head *head)
+{
+	struct seplist_chunk* cur_chunk, *newchunk;
+	void** meta = head->meta;
+	void** insert_loc;
+	size_t cidx;
+	
+	smapdisable();
+	seplist_check_integrity(head);
+
+	if(!meta)
+	{
+		newchunk = new_seplist_chunk_init();
+		newchunk->obj[SEPLIST_CHUNK_ENTRIES-1] = &head->meta;
+		meta = &newchunk->obj[SEPLIST_CHUNK_ENTRIES-1];
+		WRITE_ONCE(head->meta, meta);
+	}
+
+	cur_chunk = seplist_meta_to_chunk(meta);
+	cidx = seplist_meta_to_chunkidx(meta);
+
+	if(cidx == SEPLIST_CHUNK_ENTRIES - 1)
+	{
+		insert_loc = seplist_get_last_freespot_in_chunk(cur_chunk->next, 0);
+
+		if(insert_loc)
+		{
+			seplist_insert_obj(insert_loc, new);
+			smapenable();
+			return;
+		}
+	}
+	else
+	{
+		insert_loc = seplist_get_last_freespot_in_chunk(cur_chunk, cidx+1);
+
+		if(insert_loc)
+		{
+			seplist_insert_obj(insert_loc, new);
+			smapenable();
+			return;
+		}
+	}
+
+
+	/* No space could be found, so create new space by splitting */
+	insert_loc = seplist_split_chunk(meta);
+	insert_loc = seplist_get_last_freespot_in_chunk(seplist_meta_to_chunk(insert_loc), seplist_meta_to_chunkidx(insert_loc));
+	seplist_insert_obj(insert_loc, new);
+	smapenable();
+}
+
+
+/**
+ * seplist_add_tail - add a new entry
+ * @new: new entry to be added
+ * @head: list head to add it before
+ *
+ * Insert a new entry before the specified head.
+ * This is useful for implementing queues.
+ */
+static inline void seplist_add_tail(struct seplist_head *new, struct seplist_head *head)
+{
+	struct seplist_chunk* cur_chunk, *newchunk;
+	void** meta = head->meta;
+	void** insert_loc;
+	size_t cidx;
+
+	smapdisable();
+	seplist_check_integrity(head);
+
+	if(!meta)
+	{
+		newchunk = new_seplist_chunk_init();
+		newchunk->obj[0] = &head->meta;
+		meta = &newchunk->obj[0];
+		WRITE_ONCE(head->meta, meta);
+	}
+
+	cur_chunk = seplist_meta_to_chunk(meta);
+	cidx = seplist_meta_to_chunkidx(meta);
+
+	if(cidx)
+	{
+		insert_loc = seplist_get_first_freespot_in_chunk(cur_chunk, cidx-1);
+
+		if(insert_loc)
+		{
+			seplist_insert_obj(insert_loc, new);
+			smapenable();
+			return;
+		}
+		/* No space could be found, so create new space */
+		insert_loc = seplist_split_chunk(meta-1);
+		insert_loc = seplist_get_first_freespot_in_chunk(seplist_meta_to_chunk(insert_loc), seplist_meta_to_chunkidx(insert_loc));
+		seplist_insert_obj(insert_loc, new);
+	}
+	else
+	{
+		/* Cannot find space in current chunk, look in prev one */
+		insert_loc = seplist_get_first_freespot_in_chunk(cur_chunk->prev, SEPLIST_CHUNK_ENTRIES - 1);
+
+		if(insert_loc)
+		{
+			seplist_insert_obj(insert_loc, new);
+			smapenable();
+			return;
+		}
+
+		/* No space could be found, so create new space */
+		insert_loc = seplist_split_chunk(&cur_chunk->prev->obj[SEPLIST_CHUNK_ENTRIES-1]);
+		insert_loc = seplist_get_first_freespot_in_chunk(seplist_meta_to_chunk(insert_loc), seplist_meta_to_chunkidx(insert_loc));
+		seplist_insert_obj(insert_loc, new);
+	}
+	smapenable();
+}
+
+
+
+
+
+
+
+
+

diff --git a/lkm/lkm-metaslab-stress.c b/lkm/lkm-metaslab-stress.c
new file mode 100644
index 000000000000..224260eabd64
--- /dev/null
+++ b/lkm/lkm-metaslab-stress.c
@@ -0,0 +1,130 @@
+#include <linux/init.h>
+#include <linux/module.h>
+#include <linux/fs.h>
+#include <linux/cdev.h>
+#include <linux/slab.h>
+#include <linux/types.h>
+#include <linux/uaccess.h>
+#include <linux/cred.h>
+#include <linux/kallsyms.h>
+
+#include <linux/cpu.h>
+
+#define DEBUG
+
+#ifdef DEBUG
+#define lkm_printk(fmt, ...) printk("[LKM] %s: "#fmt, __FUNCTION__, ##__VA_ARGS__)
+#else
+#define lkm_printk(fmt, ...) (void)fmt
+#endif
+
+#define NR_SIZED_CACHES    5
+#define NR_CACHES_PER_SIZE 5
+#define NR_OBJS_PER_CACHE  5
+
+static struct kmem_cache *kmem_cache_pool[NR_SIZED_CACHES][NR_CACHES_PER_SIZE];
+
+static struct kmem_cache *large_kmem_cache_pool[NR_SIZED_CACHES][NR_CACHES_PER_SIZE];
+
+static void *objects_pool[NR_SIZED_CACHES][NR_CACHES_PER_SIZE][NR_OBJS_PER_CACHE];
+
+static void *large_objects_pool[NR_SIZED_CACHES][NR_CACHES_PER_SIZE][NR_OBJS_PER_CACHE];
+
+
+static __init int lkm_init(void) {
+	int i, j, k;
+	char kmem_cache_name[64];
+	
+	lkm_printk("Initializing");
+
+	for (i = 0; i < NR_SIZED_CACHES; i++) {
+		for (j = 0; j < NR_CACHES_PER_SIZE; j++) {
+			int kmem_cache_size = (i + 1) * 8;
+
+			sprintf(kmem_cache_name, "kmem_cache_stress-%d-%d", kmem_cache_size, j);
+
+			kmem_cache_pool[i][j] = kmem_cache_create(kmem_cache_name,
+					kmem_cache_size,
+					0, // let natural alignment
+					SLAB_ISOLATE_FP,
+					NULL);
+			
+			if (!kmem_cache_pool[i][j]) {
+				lkm_printk("Error creating %s", kmem_cache_name);
+				return -1;
+			}
+		}
+	}
+	
+	for (i = 0; i < NR_SIZED_CACHES; i++) {
+		for (j = 0; j < NR_CACHES_PER_SIZE; j++) {
+			int kmem_cache_size = 8192 - 8 * i;
+
+			sprintf(kmem_cache_name, "large_kmem_cache_stress-%d-%d", kmem_cache_size, j);
+
+			large_kmem_cache_pool[i][j] = kmem_cache_create(kmem_cache_name,
+					kmem_cache_size,
+					0, // let natural alignment
+					SLAB_ISOLATE_FP,
+					NULL);
+			
+			if (!large_kmem_cache_pool[i][j]) {
+				lkm_printk("Error creating %s", kmem_cache_name);
+				return -1;
+			}
+		}
+	}
+
+	for (i = 0; i < NR_SIZED_CACHES; i++) {
+		for (j = 0; j < NR_CACHES_PER_SIZE; j++) {
+			for (k = 0; k < NR_OBJS_PER_CACHE; k++) {
+				objects_pool[i][j][k] = kmem_cache_alloc(kmem_cache_pool[i][j], __GFP_ZERO);
+			}
+		}
+	}
+	
+	for (i = 0; i < NR_SIZED_CACHES; i++) {
+		for (j = 0; j < NR_CACHES_PER_SIZE; j++) {
+			for (k = 0; k < NR_OBJS_PER_CACHE; k++) {
+				large_objects_pool[i][j][k] = kmem_cache_alloc(large_kmem_cache_pool[i][j], __GFP_ZERO);
+			}
+		}
+	}
+	
+	for (i = 0; i < NR_SIZED_CACHES; i++) {
+		for (j = 0; j < NR_CACHES_PER_SIZE; j++) {
+			for (k = 0; k < NR_OBJS_PER_CACHE; k++) {
+				kfree(objects_pool[i][j][k]);
+			}
+		}
+	}
+	
+	for (i = 0; i < NR_SIZED_CACHES; i++) {
+		for (j = 0; j < NR_CACHES_PER_SIZE; j++) {
+			for (k = 0; k < NR_OBJS_PER_CACHE; k++) {
+				kfree(large_objects_pool[i][j][k]);
+			}
+		}
+	}
+
+	return 0;
+}
+
+static __exit void lkm_exit(void) {
+	int i, j;
+	
+	lkm_printk("Exiting");
+
+	for (i = 0; i < NR_SIZED_CACHES; i++)
+		for (j = 0; j < NR_CACHES_PER_SIZE; j++)
+			kmem_cache_destroy(kmem_cache_pool[i][j]);
+	
+	for (i = 0; i < NR_SIZED_CACHES; i++)
+		for (j = 0; j < NR_CACHES_PER_SIZE; j++)
+			kmem_cache_destroy(large_kmem_cache_pool[i][j]);
+}
+
+module_init(lkm_init);
+module_exit(lkm_exit);
+
+MODULE_LICENSE("GPL");

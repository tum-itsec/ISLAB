+/**
+ * seplist_is_first -- tests whether @seplist is the first entry in seplist @head
+ * @seplist: the entry to test
+ * @head: the head of the seplist
+ */
+static inline int seplist_is_first(const struct seplist_head *seplist,
+					const struct seplist_head *head)
+{
+	return seplist_prev(seplist) == head;
+}
+
+/**
+ * seplist_is_singular - tests whether a seplist has just one entry.
+ * @head: the seplist to test.
+ */
+static inline int seplist_is_singular(const struct seplist_head *head)
+{
+	return !seplist_empty(head) && (seplist_next_unchecked(head) == seplist_prev_unchecked(head));
+}
+
+
+/**
+ * seplist_is_last - tests whether @seplist is the last entry in seplist @head
+ * @seplist: the entry to test
+ * @head: the head of the seplist
+ */
+static inline int seplist_is_last(const struct seplist_head *seplist,
+				const struct seplist_head *head)
+{
+	return seplist_next(seplist) == head;
+}

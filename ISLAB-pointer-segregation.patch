+/* Returns and removes the most recent free entry in the list
+ * This function assumes irq is disabled and no concurrent access can occur */
+static inline void* pop_freepointer(struct kmem_cache *s, struct page *slab, void **freelist)
+{
+	void *obj;
+	uint16_t offset;
+	void *cur = *freelist;
+
+	offset = kmem_freelist_get_offset(cur);
+
+	VM_BUG_ON(offset == 0xffff);
+	VM_BUG_ON(offset > slab->objects);
+
+	obj = slab_obj_by_index(offset, s, slab);
+
+	if (kmem_freelist_last_entry(cur)) {
+		struct kmem_freelist_node *cur_node = kmem_freelist_get_node(cur);
+		*freelist = cur_node->next;
+
+		_free_freelist_node(cur_node, slab, s, obj, freelist);
+	}
+	else {
+		*freelist = kmem_freelist_decreased_fill(cur);
+	}
+
+
+
+	return obj;
+}

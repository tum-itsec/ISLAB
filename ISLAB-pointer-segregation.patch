+ * primitives such as seplist_add_rcu() as long as it's guarded by rcu_read_lock().
+ */
+#define seplist_entry_rcu(ptr, type, member) seplist_entry(ptr, type, member)
+
+
+/**
+ * seplist_is_last_rcu - tests whether @list is the last entry in list @head
+ * @list: the entry to test
+ * @head: the head of the seplist
+ */
+static inline int seplist_is_last_rcu(const struct seplist_head *list,
+				const struct seplist_head *head)
+{
+	return (seplist_next_rcu(list)) == head;
+}
+
+/**
+ * seplist_first_or_null_rcu - get the first element from a seplist
+ * @ptr:        the seplist head to take the element from.
+ * @type:       the type of the struct this is embedded in.
+ * @member:     the name of the seplist_head within the struct.
+ *
+ * Note that if the seplist is empty, it returns NULL.
+ *
+ * This primitive may safely run concurrently with the _rcu seplist-mutation
+ * primitives such as seplist_add_rcu() as long as it's guarded by rcu_read_lock().
+ */
+#define seplist_first_or_null_rcu(ptr, type, member) \
+({ \
+	struct seplist_head *__ptr = (ptr); \
+	struct seplist_head *__next = seplist_next_rcu(__ptr); \
+	likely(__ptr != __next) ? seplist_entry_rcu(__next, type, member) : NULL; \
+})
+
+/**
+ * seplist_next_or_null_rcu - get the first element from a seplist
+ * @head:	the head for the seplist.
+ * @ptr:        the seplist head to take the next element from.
+ * @type:       the type of the struct this is embedded in.
+ * @member:     the name of the seplist_head within the struct.
+ *
+ * Note that if the ptr is at the end of the seplist, NULL is returned.
+ *
+ * This primitive may safely run concurrently with the _rcu seplist-mutation
+ * primitives such as seplist_add_rcu() as long as it's guarded by rcu_read_lock().
+ */
+#define seplist_next_or_null_rcu(head, ptr, type, member) \
+({ \
+	struct seplist_head *__head = (head); \
+	struct seplist_head *__ptr = (ptr); \
+	struct seplist_head *__next = seplist_next_rcu(__ptr); \
+	likely(__next != __head) ? seplist_entry_rcu(__next, type, \
+						  member) : NULL; \
+})
+
+/**
+ * seplist_for_each_entry_rcu	-	iterate over rcu seplist of given type
+ * @pos:	the type * to use as a loop cursor.
+ * @head:	the head for your seplist.
+ * @member:	the name of the seplist_head within the struct.
+ * @cond:	optional lockdep expression if called from non-RCU protection.
+ *
+ * This seplist-traversal primitive may safely run concurrently with
+ * the _rcu seplist-mutation primitives such as seplist_add_rcu()
+ * as long as the traversal is guarded by rcu_read_lock().
+ */
+#define seplist_for_each_entry_rcu(pos, head, member, cond...)		\
+	for (__seplist_check_rcu(dummy, ## cond, 0),			\
+	     pos = seplist_entry(seplist_next_rcu(head), typeof(*pos), member);	\
+		&pos->member != (head);					\
+		pos = seplist_next_entry_rcu_unchecked(pos, member))
+
+/**
+ * seplist_for_each_entry_srcu	-	iterate over rcu seplist of given type
+ * @pos:	the type * to use as a loop cursor.
+ * @head:	the head for your seplist.
+ * @member:	the name of the seplist_head within the struct.
+ * @cond:	lockdep expression for the lock required to traverse the seplist.
+ *
+ * This seplist-traversal primitive may safely run concurrently with
+ * the _rcu seplist-mutation primitives such as seplist_add_rcu()
+ * as long as the traversal is guarded by srcu_read_lock().
+ * The lockdep expression srcu_read_lock_held() can be passed as the
+ * cond argument from read side.
+ */
+#define seplist_for_each_entry_srcu(pos, head, member, cond)		\
+	for (__seplist_check_srcu(cond),					\
+	     pos = seplist_entry(seplist_next_rcu(head), typeof(*pos), member);	\
+		&pos->member != (head);					\
+		pos = seplist_next_entry_rcu_unchecked(pos, member))
+
+
+
+/**
+ * seplist_entry_lockless - get the struct for this entry
+ * @ptr:        the &struct seplist_head pointer.
+ * @type:       the type of the struct this is embedded in.
+ * @member:     the name of the seplist_head within the struct.
+ *
+ * This primitive may safely run concurrently with the _rcu
+ * seplist-mutation primitives such as seplist_add_rcu(), but requires some
+ * implicit RCU read-side guarding.  One example is running within a special
+ * exception-time environment where preemption is disabled and where lockdep
+ * cannot be invoked.  Another example is when items are added to the seplist,
+ * but never deleted.
+ */
+#define seplist_entry_lockless(ptr, type, member) seplist_entry(ptr, type, member)
+
+/**
+ * seplist_for_each_entry_lockless - iterate over rcu seplist of given type
+ * @pos:	the type * to use as a loop cursor.
+ * @head:	the head for your seplist.
+ * @member:	the name of the seplist_struct within the struct.
+ *
+ * This primitive may safely run concurrently with the _rcu
+ * seplist-mutation primitives such as seplist_add_rcu(), but requires some
+ * implicit RCU read-side guarding.  One example is running within a special
+ * exception-time environment where preemption is disabled and where lockdep
+ * cannot be invoked.  Another example is when items are added to the seplist,
+ * but never deleted.
+ */
+#define seplist_for_each_entry_lockless(pos, head, member) \
+	for (pos = seplist_entry_lockless(seplist_next_rcu(head), typeof(*pos), member); \
+	     &pos->member != (head); \
+	     pos = seplist_next_entry_rcu_unchecked(pos, member))
+
+
+/**
+ * seplist_for_each_entry_continue_rcu - continue iteration over seplist of given type
+ * @pos:	the type * to use as a loop cursor.
+ * @head:	the head for your seplist.
+ * @member:	the name of the seplist_head within the struct.
+ *
+ * Continue to iterate over seplist of given type, continuing after
+ * the current position which must have been in the seplist when the RCU read
+ * lock was taken.
+ * This would typically require either that you obtained the node from a
+ * previous walk of the seplist in the same RCU read-side critical section, or
+ * that you held some sort of non-RCU reference (such as a reference count)
+ * to keep the node alive *and* in the seplist.
+ *
+ * This iterator is similar to seplist_for_each_entry_from_rcu() except
+ * this starts after the given position and that one starts at the given
+ * position.
+ */
+#define seplist_for_each_entry_continue_rcu(pos, head, member) 		\
+	for (pos = seplist_next_entry_rcu(pos, member); \
+	     &pos->member != (head);	\
+	     pos = seplist_next_entry_rcu_unchecked(pos, member))
+
+/**
+ * seplist_for_each_entry_from_rcu - iterate over a seplist from current point
+ * @pos:	the type * to use as a loop cursor.
+ * @head:	the head for your seplist.
+ * @member:	the name of the seplist_node within the struct.
+ *
+ * Iterate over the tail of a seplist starting from a given position,
+ * which must have been in the seplist when the RCU read lock was taken.
+ * This would typically require either that you obtained the node from a
+ * previous walk of the seplist in the same RCU read-side critical section, or
+ * that you held some sort of non-RCU reference (such as a reference count)
+ * to keep the node alive *and* in the seplist.
+ *
+ * This iterator is similar to seplist_for_each_entry_continue_rcu() except
+ * this starts from the given position and that one starts from the position
+ * after the given position.
+ */
+#define seplist_for_each_entry_from_rcu(pos, head, member)			\
+	for (; &(pos)->member != (head);					\
+		pos = seplist_next_entry_rcu_unchecked(pos, member))
+
+
+
+#ifdef CONFIG_SEPLIST_MAGAZINE
+
+static inline void __seplist_splice_chunks_rcu(struct seplist_chunk* spliced,
+		struct seplist_chunk* prev, struct seplist_chunk* next)
+{
+	struct seplist_chunk* first = spliced;
+	struct seplist_chunk* last = spliced->prev;
+
+	ASSERT_EXCLUSIVE_ACCESS(*first);
+	ASSERT_EXCLUSIVE_ACCESS(*last);
+
+	/*
+	 * Readers are finished with the source seplist, so perform splice.
+	 * The order is important if the new seplist is global and accessible
+	 * to concurrent RCU readers.  Note that RCU readers are not
+	 * permitted to traverse the prev pointers without excluding
+	 * this function.
+	 */
+
+	last->next = next;
+	first->prev = prev;
+	next->prev = last;
+	rcu_assign_pointer(seplist_chunk_next_rcu(prev), first);
+}
+
+
+static inline void __seplist_splice_rcu(void **listmeta,
+		void **headmeta)
+{
+	struct seplist_chunk *headchunk, *listchunk;
+
+	listchunk = seplist_meta_to_chunk(listmeta);
+
+	/* headchunk is gonna change on split */
+	if(unlikely(!seplist_is_head_last_in_chunk(headmeta)))
+		headchunk = seplist_meta_to_chunk(seplist_split_chunk_rcu(headmeta));
+	else
+		headchunk = seplist_meta_to_chunk(headmeta);
+
+	if(unlikely(!seplist_is_head_last_in_chunk(listmeta)))
+		seplist_split_chunk(listmeta);
+
+	__seplist_splice_chunks_rcu(listchunk->next, headchunk, headchunk->next);
+}
+
+/*
+ * seplist_splice_init_rcu - splice an RCU-protected seplist into an existing seplist,
+ *                        designed for stacks.
+ * @seplist:	the RCU-protected seplist to splice
+ * @head:	the place in the existing seplist to splice the first seplist into
+ * @sync:	synchronize_rcu, synchronize_rcu_expedited, ...
+ */
+static inline void seplist_splice_init_rcu(struct seplist_head *list,
+					struct seplist_head *head,
+					void (*sync)(void))
+{
+	void **listmeta, **splicemeta;
+	struct seplist_chunk *chunk;
+	if(seplist_empty(list))
+		return;
+
+	smapdisable();
+	seplist_check_integrity(list);
+	seplist_check_integrity(head);
+
+
+	/* Steal the list */
+	listmeta = list->meta;
+	INIT_SEPLIST_HEAD_RCU(list);
+
+	/* Make sure noone is reading the old list */
+	sync();
+
+
+	if(!head->meta)
+	{
+		*listmeta = (void*)head;
+		head->meta = listmeta;
+		smapenable();
+		return;
+	}
+
+	chunk = seplist_meta_to_chunk(listmeta);
+	/* Delete previous head */
+	*listmeta = (void*)seplist_rcu_delptr(list);
+
+	if (sepchunk_unused(chunk)) {
+		splicemeta = &chunk->prev->obj[SEPLIST_CHUNK_ENTRIES-1];
+		chunk->next->prev = chunk->prev;
+		chunk->prev->next = chunk->next;
+		/* Use deferred del to sync with potentially other synchronous deferred dels */
+		__seplist_del_deferred_empty_ptr(listmeta);
+	}
+	else {
+		splicemeta = listmeta;
+		*listmeta = NULL;
+	}
+
+	__seplist_splice_rcu(listmeta, head->meta);
+	smapenable();
+}
+
+/**
+ * seplist_splice_tail_init_rcu - splice an RCU-protected seplist into an existing
+ *                             seplist, designed for queues.
+ * @seplist:	the RCU-protected seplist to splice
+ * @head:	the place in the existing seplist to splice the first seplist into
+ * @sync:	synchronize_rcu, synchronize_rcu_expedited, ...
+ */
+static inline void seplist_splice_tail_init_rcu(struct seplist_head *list,
+					     struct seplist_head *head,
+					     void (*sync)(void))
+{
+	void **listmeta, **splicemeta;
+	struct seplist_chunk *chunk;
+	//printk("splice_tail list=%llx, head=%llx\n", (u64)list, (u64)head);
+	if(seplist_empty(list))
+		return;
+	smapdisable();
+	seplist_check_integrity(list);
+	seplist_check_integrity(head);
+
+
+	/* Steal the list */
+	listmeta = list->meta;
+	INIT_SEPLIST_HEAD_RCU(list);
+
+	/* Make sure noone is reading the old list */
+	sync();
+
+	if(!head->meta)
+	{
+		*listmeta = (void*)head;
+		head->meta = listmeta;
+		smapenable();
+		return;
+	}
+
+	chunk = seplist_meta_to_chunk(listmeta);
+	/* Delete previous head */
+	*listmeta = (void*)seplist_rcu_delptr(list);
+
+	if (sepchunk_unused(chunk)) {
+		splicemeta = &chunk->prev->obj[SEPLIST_CHUNK_ENTRIES-1];
+		chunk->next->prev = chunk->prev;
+		chunk->prev->next = chunk->next;
+		/* Use deferred del to sync with potentially other synchronous deferred dels */
+		__seplist_del_deferred_empty_ptr(listmeta);
+	}
+	else {
+		splicemeta = seplist_prev_metaspot(listmeta);
+		*listmeta = NULL;
+	}
+
+	__seplist_splice_rcu(splicemeta, seplist_prev_metaspot(head->meta));
+	smapenable();
+}
+
+
+#elif defined(CONFIG_SEPLIST_SINGLE)
+
+/**
+ * __seplist_splice_init_rcu - join an RCU-protected seplist into an existing seplist.
+ * @seplist:	the RCU-protected seplist to splice
+ * @prev:	points to the last element of the existing seplist
+ * @next:	points to the first element of the existing seplist
+ * @sync:	synchronize_rcu, synchronize_rcu_expedited, ...
+ *
+ * The seplist pointed to by @prev and @next can be RCU-read traversed
+ * concurrently with this function.
+ *
+ * Note that this function blocks.
+ *
+ * Important note: the caller must take whatever action is necessary to prevent
+ * any other updates to the existing seplist.  In principle, it is possible to
+ * modify the seplist as soon as sync() begins execution. If this sort of thing
+ * becomes necessary, an alternative version based on call_rcu() could be
+ * created.  But only if -really- needed -- there is no shortage of RCU API
+ * members.
+ */
+static inline void __seplist_splice_init_rcu(struct seplist_head *seplist,
+					  struct seplist_chunk *prev,
+					  struct seplist_chunk *next,
+					  void (*sync)(void))
+{
+	struct seplist_chunk *seplist_chunk = (struct seplist_chunk *)seplist->meta;
+	struct seplist_chunk* first = seplist_chunk->next;
+	struct seplist_chunk* last = seplist_chunk->prev;
+
+	/*
+	 * "first" and "last" tracking seplist, so initialize it.  RCU readers
+	 * have access to this seplist, so we must use INIT_seplist_HEAD_RCU()
+	 * instead of INIT_seplist_HEAD().
+	 */
+
+	INIT_SEPLIST_HEAD_RCU(seplist);
+
+	/*
+	 * At this point, the seplist body still points to the source seplist.
+	 * Wait for any readers to finish using the seplist before splicing
+	 * the seplist body into the new seplist.  Any new readers will see
+	 * an empty seplist.
+	 */
+
+	sync();
+	ASSERT_EXCLUSIVE_ACCESS(*first);
+	ASSERT_EXCLUSIVE_ACCESS(*last);
+
+	last->next = next;
+	first->prev = prev;
+	next->prev = last;
+	rcu_assign_pointer(seplist_chunk_next_rcu(prev), first);
+	__seplist_deferred_del_chunk(seplist_chunk);
+}
+
+
+
+
+/*
+ * seplist_splice_init_rcu - splice an RCU-protected seplist into an existing seplist,
+ *                        designed for stacks.
+ * @seplist:	the RCU-protected seplist to splice
+ * @head:	the place in the existing seplist to splice the first seplist into
+ * @sync:	synchronize_rcu, synchronize_rcu_expedited, ...
+ */
+static inline void seplist_splice_init_rcu(struct seplist_head *list,
+					struct seplist_head *head,
+					void (*sync)(void))
+{
+	struct seplist_chunk* head_chunk = (struct seplist_chunk *)READ_ONCE(head->meta);
+	if(seplist_empty(list))
+		return;
+
+	smapdisable();
+	seplist_check_integrity(list);
+	seplist_check_integrity(head);
+
+
+	__seplist_splice_init_rcu(list, head_chunk, head_chunk->next, sync);
+	smapenable();
+}
+
+/**
+ * seplist_splice_tail_init_rcu - splice an RCU-protected seplist into an existing
+ *                             seplist, designed for queues.
+ * @seplist:	the RCU-protected seplist to splice
+ * @head:	the place in the existing seplist to splice the first seplist into
+ * @sync:	synchronize_rcu, synchronize_rcu_expedited, ...
+ */
+static inline void seplist_splice_tail_init_rcu(struct seplist_head *list,
+					     struct seplist_head *head,
+					     void (*sync)(void))
+{
+	struct seplist_chunk* head_chunk = (struct seplist_chunk *)READ_ONCE(head->meta);
+	if(seplist_empty(list))
+		return;
+	smapdisable();
+	seplist_check_integrity(list);
+	seplist_check_integrity(head);
+
+
+	__seplist_splice_init_rcu(list, head_chunk->prev, head_chunk, sync);
+	smapenable();
+}
+
+
+
+#endif /* CONFIG_SEPLIST_SINGLE */
+
+
+#endif
+

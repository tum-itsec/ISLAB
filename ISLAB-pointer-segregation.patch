+static inline int seplist_is_head_last_in_chunk(void **meta)
+{
+	ssize_t cidx;
+	ssize_t last_entry;
+	struct seplist_chunk* chunk;
+
+	cidx = seplist_meta_to_chunkidx(meta);
+
+	/* Fast path */
+	if(cidx == SEPLIST_CHUNK_ENTRIES - 1)
+		return 1;
+
+	chunk = seplist_meta_to_chunk(meta);
+
+	for(last_entry = SEPLIST_CHUNK_ENTRIES - 1; !seplist_entry_has_entry(chunk->obj[last_entry]) && last_entry != cidx; --last_entry);
+	return last_entry == cidx;
+}
+
+static inline int seplist_is_head_first_in_chunk(void **meta)
+{
+	ssize_t cidx;
+	ssize_t first_entry;
+	struct seplist_chunk* chunk;
+
+	cidx = seplist_meta_to_chunkidx(meta);
+
+	/* Fast path */
+	if(cidx == 0)
+		return 1;
+
+	chunk = seplist_meta_to_chunk(meta);
+
+	for(first_entry = 0; !seplist_entry_has_entry(chunk->obj[first_entry]) && first_entry != cidx; ++first_entry);
+	return first_entry == cidx;
+}

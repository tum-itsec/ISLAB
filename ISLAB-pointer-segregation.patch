+	/* before: [seplist1]->e[0]->e[1]->e[4], [seplist2]->e[2]->e[3] */
+	seplist_splice_init_rcu(&seplist2, &entries[1], synchronize_rcu);
+	/* after: [seplist1]->e[0]->e[1]->e[2]->e[3]->e[4], [seplist2] empty */
+
+	seplist_for_each(cur, &seplist1) {

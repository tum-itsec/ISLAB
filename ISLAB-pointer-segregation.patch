+#ifdef CONFIG_SEPLIST_MAGAZINE
+
+static inline void** seplist_split_chunk_rcu(void **chunk_pos)
+{
+	ssize_t i, j, cidx;
+	int cur_chunk_is_singleton;
+	struct seplist_chunk *cur_chunk, *new_chunk_cur, *new_chunk_next;
+	void **r;
+	cur_chunk = seplist_meta_to_chunk(chunk_pos);
+	cidx = seplist_meta_to_chunkidx(chunk_pos);
+
+	/* Special fast case */
+	if(cidx == SEPLIST_CHUNK_ENTRIES - 1)
+	{
+		new_chunk_next = new_seplist_chunk();
+		__seplist_add_chunk_rcu(new_chunk_next, cur_chunk, cur_chunk->next);
+		return &new_chunk_next->obj[0];
+	}
+
+	cur_chunk_is_singleton = cur_chunk->next == cur_chunk;
+
+	new_chunk_cur = new_seplist_chunk();
+	new_chunk_next = new_seplist_chunk();
+
+	new_chunk_next->prev = new_chunk_cur;
+	new_chunk_cur->next = new_chunk_next;
+
+	if(!cur_chunk_is_singleton)
+	{
+		new_chunk_next->next = cur_chunk->next;
+		new_chunk_cur->prev = cur_chunk->prev;
+	}
+	else
+	{
+		new_chunk_next->next = new_chunk_cur;
+		new_chunk_cur->prev = new_chunk_next;
+	}
+
+	/* Initialize meta data */
+	memcpy(new_chunk_cur->obj, cur_chunk->obj, (cidx+1) * sizeof(void*));
+	memcpy(new_chunk_next->obj, &cur_chunk->obj[cidx+1], (SEPLIST_CHUNK_ENTRIES - cidx - 1) * sizeof(void*));
+
+	/* Move linking on objects now to new list, starting at last entries */
+
+	for(i = SEPLIST_CHUNK_ENTRIES-1, j = (SEPLIST_CHUNK_ENTRIES - cidx - 2); i > cidx; --i, --j)
+	{
+		r = new_chunk_next->obj[j];
+		if(seplist_entry_has_entry(r))
+		{
+			rcu_assign_pointer(*(void __rcu**)r, &new_chunk_next->obj[j]);
+		}
+	}
+
+	for(i = cidx; i >= 0; --i)
+	{
+		r = new_chunk_cur->obj[i];
+		if(seplist_entry_has_entry(r))
+		{
+			rcu_assign_pointer(*(void __rcu**)r, &new_chunk_cur->obj[i]);
+		}
+	}
+
+	if(!cur_chunk_is_singleton)
+	{
+		new_chunk_next->next->prev = new_chunk_next;
+		rcu_assign_pointer(seplist_chunk_next_rcu(cur_chunk->prev), new_chunk_cur);
+	}
+
+	seplist_rcu_defer_call(__seplist_deferred_del_chunk, cur_chunk);
+
+	return &new_chunk_cur->obj[cidx+1];
+}

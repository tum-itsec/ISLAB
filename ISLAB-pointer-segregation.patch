+static inline void __seplist_splice(struct seplist_chunk* spliced,
+		struct seplist_chunk* prev, struct seplist_chunk* next)
+{
+	struct seplist_chunk* first = spliced->next;
+	struct seplist_chunk* last = spliced->prev;
+
+	first->prev = prev;
+	prev->next = first;
+
+	last->next = next;
+	next->prev = last;
+}
+
+
+/**
+ * seplist_splice - join two lists, this is designed for stacks
+ * @list: the new list to add.
+ * @head: the place to add it in the first list.
+ */
+static inline void seplist_splice(const struct seplist_head *list,
+		struct seplist_head *head)
+{
+	struct seplist_chunk *listmeta, *headmeta;
+
+	listmeta = (struct seplist_chunk *)list->meta;
+	headmeta = (struct seplist_chunk *)head->meta;
+
+	if(seplist_empty(list)) {
+		return;
+	}
+	smapdisable();
+	seplist_check_integrity(list);
+	seplist_check_integrity(head);
+
+
+	if(!headmeta)
+	{
+		listmeta->obj = head;
+		head->meta = (void*)listmeta;
+		smapenable();
+		return;
+	}
+
+	__seplist_splice(listmeta, headmeta, headmeta->next);
+	__seplist_free_chunk(listmeta);
+	smapenable();
+}
+
+/**
+ * seplist_splice - join two lists, this is designed for stacks
+ * @list: the new list to add.
+ * @head: the place to add it in the first list.
+ */
+static inline void seplist_splice_tail(const struct seplist_head *list,
+		struct seplist_head *head)
+{
+	struct seplist_chunk *listmeta, *headmeta;
+	
+	listmeta = (struct seplist_chunk *)list->meta;
+	headmeta = (struct seplist_chunk *)head->meta;
+	if(seplist_empty(list)) {
+		return;
+	}
+	smapdisable();
+	seplist_check_integrity(list);
+	seplist_check_integrity(head);
+
+
+	if(!headmeta)
+	{
+		listmeta->obj = head;
+		head->meta = (void*)listmeta;
+		smapenable();
+		return;
+	}
+
+	__seplist_splice(listmeta, headmeta->prev, headmeta);
+	__seplist_free_chunk(listmeta);
+	smapenable();
+}
+#endif /* CONFIG_SEPLIST_SINGLE */
+
+/**
+ * seplist_splice_init - join two seplists and reinitialise the emptied seplist.
+ * @seplist: the new seplist to add.
+ * @head: the place to add it in the first seplist.
+ *
+ * The seplist at @seplist is reinitialised
+ */
+static inline void seplist_splice_init(struct seplist_head *seplist,
+				    struct seplist_head *head)
+{
+	if (!seplist_empty(seplist)) {
+		seplist_splice(seplist, head);
+		INIT_SEPLIST_HEAD(seplist);
+	}
+}
+
+
+/**
+ * seplist_splice_tail_init - join two seplists and reinitialise the emptied seplist
+ * @seplist: the new seplist to add.
+ * @head: the place to add it in the first seplist.
+ *
+ * Each of the seplists is a queue.
+ * The seplist at @seplist is reinitialised
+ */
+static inline void seplist_splice_tail_init(struct seplist_head *seplist,
+					 struct seplist_head *head)
+{
+	if (!seplist_empty(seplist)) {
+		seplist_splice_tail(seplist, head);
+		INIT_SEPLIST_HEAD(seplist);
+	}
+}

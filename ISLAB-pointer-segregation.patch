+/**
+ * seplist_splice - join two lists, this is designed for stacks
+ * @list: the new list to add.
+ * @head: the place to add it in the first list.
+ */
+static inline void seplist_splice(const struct seplist_head *list,
+		struct seplist_head *head)
+{
+	void **listmeta;
+	struct seplist_chunk *chunk;
+
+	if(seplist_empty(list))
+		return;
+	smapdisable();
+
+	seplist_check_integrity(list);
+	seplist_check_integrity(head);
+
+	listmeta = list->meta;
+	chunk = seplist_meta_to_chunk(listmeta);
+
+
+	if(!head->meta)
+	{
+		*listmeta = (void*)head;
+		head->meta = listmeta;
+		smapenable();
+		return;
+	}
+
+	__seplist_splice(listmeta, head->meta);
+
+	/* Delete previous head */
+	*listmeta = (void*)NULL;
+
+	/* Check if chunk empty now and clean up if necessary */
+	if(seplist_is_chunk_empty(chunk))
+		__seplist_del_chunk(chunk);
+	smapenable();
+}

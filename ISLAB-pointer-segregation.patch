new file mode 100644
index 000000000000..cf1acbbaabe1
--- /dev/null
+++ b/lib/seplist_sort.c
@@ -0,0 +1,920 @@
+// SPDX-License-Identifier: GPL-2.0
+#include <linux/kernel.h>
+#include <linux/bug.h>
+#include <linux/compiler.h>
+#include <linux/export.h>
+#include <linux/string.h>
+#include <linux/seplist_sort.h>
+#include <linux/seplist.h>
+#include <linux/ksmap.h>
+
+#ifdef CONFIG_SEPLIST_SINGLE
+
+typedef int __attribute__((nonnull(2,3))) (*cmp_func)(void *,
+		struct seplist_head const *, struct seplist_head const *);
+
+/*
+ * Returns a list organized in an intermediate format suited
+ * to chaining of merge() calls: null-terminated, no reserved or
+ * sentinel head node, "prev" links not maintained.
+ */
+__attribute__((nonnull(2,3,4)))
+static struct seplist_chunk *merge(void *priv, cmp_func cmp,
+				struct seplist_chunk *a, struct seplist_chunk *b)
+{
+	struct seplist_chunk *head, **tail = &head;
+
+	for (;;) {
+		/* if equal, take 'a' -- important for sort stability */
+		if (cmp(priv, (struct seplist_head *)a->obj,
+					(struct seplist_head *)b->obj) <= 0) {
+			*tail = a;
+			tail = &a->next;
+			a = a->next;
+			if (!a) {
+				*tail = b;
+				break;
+			}
+		} else {
+			*tail = b;
+			tail = &b->next;
+			b = b->next;
+			if (!b) {
+				*tail = a;
+				break;
+			}
+		}
+	}
+	return head;
+}
+
+/*
+ * Combine final list merge with restoration of standard doubly-linked
+ * list structure.  This approach duplicates code from merge(), but
+ * runs faster than the tidier alternatives of either a separate final
+ * prev-link restoration pass, or maintaining the prev links
+ * throughout.
+ */
+__attribute__((nonnull(2,3,4,5)))
+static void merge_final(void *priv, cmp_func cmp, struct seplist_chunk *head,
+			struct seplist_chunk *a, struct seplist_chunk *b)
+{
+	struct seplist_chunk *tail = head;
+	u8 count = 0;
+
+	for (;;) {
+		/* if equal, take 'a' -- important for sort stability */
+		if (cmp(priv, (struct seplist_head *)a->obj,
+					(struct seplist_head *)b->obj) <= 0) {
+			tail->next = a;
+			a->prev = tail;
+			tail = a;
+			a = a->next;
+			if (!a)
+				break;
+		} else {
+			tail->next = b;
+			b->prev = tail;
+			tail = b;
+			b = b->next;
+			if (!b) {
+				b = a;
+				break;
+			}
+		}
+	}
+
+	/* Finish linking remainder of list b on to tail */
+	tail->next = b;
+	do {
+		/*
+		 * If the merge is highly unbalanced (e.g. the input is
+		 * already sorted), this loop may run many iterations.
+		 * Continue callbacks to the client even though no
+		 * element comparison is needed, so the client's cmp()
+		 * routine can invoke cond_resched() periodically.
+		 */
+		if (unlikely(!++count))
+			cmp(priv, (struct seplist_head *)b->obj, (struct seplist_head *)b->obj);
+		b->prev = tail;
+		tail = b;
+		b = b->next;
+	} while (b);
+
+	/* And the final links to make a circular doubly-linked list */
+	tail->next = head;
+	head->prev = tail;
+}
+
+/**
+ * seplist_sort - sort a list
+ * @priv: private data, opaque to list_sort(), passed to @cmp
+ * @head: the list to sort
+ * @cmp: the elements comparison function
+ *
+ * The comparison funtion @cmp must return > 0 if @a should sort after
+ * @b ("@a > @b" if you want an ascending sort), and <= 0 if @a should
+ * sort before @b *or* their original order should be preserved.  It is
+ * always called with the element that came first in the input in @a,
+ * and list_sort is a stable sort, so it is not necessary to distinguish
+ * the @a < @b and @a == @b cases.
+ *
+ * This is compatible with two styles of @cmp function:
+ * - The traditional style which returns <0 / =0 / >0, or
+ * - Returning a boolean 0/1.
+ * The latter offers a chance to save a few cycles in the comparison
+ * (which is used by e.g. plug_ctx_cmp() in block/blk-mq.c).
+ *
+ * A good way to write a multi-word comparison is::
+ *
+ *	if (a->high != b->high)
+ *		return a->high > b->high;
+ *	if (a->middle != b->middle)
+ *		return a->middle > b->middle;
+ *	return a->low > b->low;
+ *
+ *
+ * This mergesort is as eager as possible while always performing at least
+ * 2:1 balanced merges.  Given two pending sublists of size 2^k, they are
+ * merged to a size-2^(k+1) list as soon as we have 2^k following elements.
+ *
+ * Thus, it will avoid cache thrashing as long as 3*2^k elements can
+ * fit into the cache.  Not quite as good as a fully-eager bottom-up
+ * mergesort, but it does use 0.2*n fewer comparisons, so is faster in
+ * the common case that everything fits into L1.
+ *
+ *
+ * The merging is controlled by "count", the number of elements in the
+ * pending lists.  This is beautiully simple code, but rather subtle.
+ *
+ * Each time we increment "count", we set one bit (bit k) and clear
+ * bits k-1 .. 0.  Each time this happens (except the very first time
+ * for each bit, when count increments to 2^k), we merge two lists of
+ * size 2^k into one list of size 2^(k+1).
+ *
+ * This merge happens exactly when the count reaches an odd multiple of
+ * 2^k, which is when we have 2^k elements pending in smaller lists,
+ * so it's safe to merge away two lists of size 2^k.
+ *
+ * After this happens twice, we have created two lists of size 2^(k+1),
+ * which will be merged into a list of size 2^(k+2) before we create
+ * a third list of size 2^(k+1), so there are never more than two pending.
+ *
+ * The number of pending lists of size 2^k is determined by the
+ * state of bit k of "count" plus two extra pieces of information:
+ *
+ * - The state of bit k-1 (when k == 0, consider bit -1 always set), and
+ * - Whether the higher-order bits are zero or non-zero (i.e.
+ *   is count >= 2^(k+1)).
+ *
+ * There are six states we distinguish.  "x" represents some arbitrary
+ * bits, and "y" represents some arbitrary non-zero bits:
+ * 0:  00x: 0 pending of size 2^k;           x pending of sizes < 2^k
+ * 1:  01x: 0 pending of size 2^k; 2^(k-1) + x pending of sizes < 2^k
+ * 2: x10x: 0 pending of size 2^k; 2^k     + x pending of sizes < 2^k
+ * 3: x11x: 1 pending of size 2^k; 2^(k-1) + x pending of sizes < 2^k
+ * 4: y00x: 1 pending of size 2^k; 2^k     + x pending of sizes < 2^k
+ * 5: y01x: 2 pending of size 2^k; 2^(k-1) + x pending of sizes < 2^k
+ * (merge and loop back to state 2)
+ *
+ * We gain lists of size 2^k in the 2->3 and 4->5 transitions (because
+ * bit k-1 is set while the more significant bits are non-zero) and
+ * merge them away in the 5->2 transition.  Note in particular that just
+ * before the 5->2 transition, all lower-order bits are 11 (state 3),
+ * so there is one list of each smaller size.
+ *
+ * When we reach the end of the input, we merge all the pending
+ * lists, from smallest to largest.  If you work through cases 2 to
+ * 5 above, you can see that the number of elements we merge with a list
+ * of size 2^k varies from 2^(k-1) (cases 3 and 5 when x == 0) to
+ * 2^(k+1) - 1 (second merge of case 5 when x == 2^(k-1) - 1).
+ */
+__attribute__((nonnull(2,3)))
+void seplist_sort(void *priv, struct seplist_head *head_h,
+		int (*cmp)(void *priv, struct seplist_head *a,
+			struct seplist_head *b))
+{
+#ifdef CONFIG_SEPLIST_KSMAP
+	ksmap_disable_smap();
+#endif
+	struct seplist_chunk *head = (struct seplist_chunk *)head_h->meta, *list, *pending = NULL;
+	size_t count = 0;	/* Count of pending */
+
+	if(!head)
+		goto out;
+
+	list = head->next;
+
+	if (list == head->prev)	/* Zero or one elements */
+		goto out;
+
+	/* Convert to a null-terminated singly-linked list. */
+	head->prev->next = NULL;
+
+	/*
+	 * Data structure invariants:
+	 * - All lists are singly linked and null-terminated; prev
+	 *   pointers are not maintained.
+	 * - pending is a prev-linked "list of lists" of sorted
+	 *   sublists awaiting further merging.
+	 * - Each of the sorted sublists is power-of-two in size.
+	 * - Sublists are sorted by size and age, smallest & newest at front.
+	 * - There are zero to two sublists of each size.
+	 * - A pair of pending sublists are merged as soon as the number
+	 *   of following pending elements equals their size (i.e.
+	 *   each time count reaches an odd multiple of that size).
+	 *   That ensures each later final merge will be at worst 2:1.
+	 * - Each round consists of:
+	 *   - Merging the two sublists selected by the highest bit
+	 *     which flips when count is incremented, and
+	 *   - Adding an element from the input as a size-1 sublist.
+	 */
+	do {
+		size_t bits;
+		struct seplist_chunk **tail = &pending;
+
+		/* Find the least-significant clear bit in count */
+		for (bits = count; bits & 1; bits >>= 1)
+			tail = &(*tail)->prev;
+		/* Do the indicated merge */
+		if (likely(bits)) {
+			struct seplist_chunk *a = *tail, *b = a->prev;
+
+			a = merge(priv, (cmp_func)cmp, b, a);
+			/* Install the merged result in place of the inputs */
+			a->prev = b->prev;
+			*tail = a;
+		}
+
+		/* Move one element from input list to pending */
+		list->prev = pending;
+		pending = list;
+		list = list->next;
+		pending->next = NULL;
+		count++;
+	} while (list);
+
+	/* End of input; merge together all the pending lists. */
+	list = pending;
+	pending = pending->prev;
+	for (;;) {
+		struct seplist_chunk *next = pending->prev;
+
+		if (!next)
+			break;
+		list = merge(priv, (cmp_func)cmp, pending, list);
+		pending = next;
+	}
+	/* The final merge, rebuilding prev links */
+	merge_final(priv, (cmp_func)cmp, head, pending, list);
+out:
+#ifdef CONFIG_SEPLIST_KSMAP
+	ksmap_enable_smap();
+#endif
+	return;
+}
+EXPORT_SYMBOL(seplist_sort);
+
+#elif defined(CONFIG_SEPLIST_MAGAZINE)
+
+typedef int __attribute__((nonnull(2,3))) (*cmp_func)(void *,
+		struct seplist_head const *, struct seplist_head const *);
+
+
+static inline void sort_single_chunk(void *priv, cmp_func cmp, struct seplist_chunk *chunk)
+{
+	/* Perform simple insertion sort */
+	size_t c = 0, i, j;
+	void *new_objs[SEPLIST_CHUNK_ENTRIES];
+
+	for(i = 0; i < SEPLIST_CHUNK_ENTRIES; i++)
+	{
+		if(seplist_entry_has_entry(chunk->obj[i]))
+		{
+			for(j = 0; j < c; j++)
+			{
+				if(cmp(priv, (struct seplist_head const *)new_objs[j],
+							(struct seplist_head const *)chunk->obj[i]) > 0)
+				{
+					memmove(&new_objs[j+1], &new_objs[j], sizeof(void *) * (c-j));
+					new_objs[j] = chunk->obj[i];
+					c++;
+					break;
+				}
+			}
+			if(j == c)
+				new_objs[c++] = chunk->obj[i];
+		}
+	}
+
+	memset(chunk->obj, 0, sizeof(chunk->obj));
+	memcpy(chunk->obj, new_objs, sizeof(void *) * c);
+}
+
+static inline void fix_double_linking(struct seplist_chunk *chunk)
+{
+	size_t i;
+	for(i = 0; i < SEPLIST_CHUNK_ENTRIES; i++)
+	{
+		if(seplist_entry_has_entry(chunk->obj[i]))
+		{
+			((struct seplist_head*)chunk->obj[i])->meta = &chunk->obj[i];
+		}
+	}
+}
+
+
+struct seplist_sort_scrap_buf {
+	struct seplist_head *buf[SEPLIST_CHUNK_ENTRIES * 2];
+	size_t fill;
+};
+
+
+static inline void insert_obj_into_scrap_buf(struct seplist_sort_scrap_buf *buf,
+		struct seplist_head *obj)
+{
+	BUG_ON(buf->fill >= SEPLIST_CHUNK_ENTRIES * 2);
+	buf->buf[buf->fill++] = obj;
+}
+
+
+static inline void copy_scrap_buf_into_chunk(struct seplist_sort_scrap_buf *buf,
+		struct seplist_chunk *chunk)
+{
+	size_t cpy_amount;
+	cpy_amount = min(SEPLIST_CHUNK_ENTRIES, buf->fill);
+	memcpy(chunk->obj, buf->buf, sizeof(struct seplist_head *) * cpy_amount);
+	/* zero remaining elements */
+	memset(&chunk->obj[cpy_amount], 0, sizeof(struct seplist_head *) * (SEPLIST_CHUNK_ENTRIES - cpy_amount));
+	buf->fill -= cpy_amount;
+	memmove(buf->buf, &buf->buf[SEPLIST_CHUNK_ENTRIES], sizeof(struct seplist_head *) * buf->fill);
+}
+
+
+static inline struct seplist_chunk *pop_buf_into_scrapped(struct seplist_sort_scrap_buf *buf,
+		struct seplist_chunk **scrapped)
+{
+	struct seplist_chunk *cur = *scrapped;
+	BUG_ON(!cur);
+	*scrapped = (*scrapped)->next;
+	copy_scrap_buf_into_chunk(buf, cur);
+	return cur;
+}
+
+
+
+
+
+static inline struct seplist_head** __seplist_sort_next_sepentry(
+		struct seplist_head** cur, struct seplist_chunk **scrap_list)
+{
+	struct seplist_chunk *cur_chunk, *tmp;
+	struct seplist_head** victim;
+	int chunk_idx;
+
+	cur_chunk = seplist_meta_to_chunk(cur);
+	chunk_idx = seplist_meta_to_chunkidx(cur) + 1;
+
+	while(!(victim = seplist_get_next_used_spot_in_chunk(cur_chunk, chunk_idx)))
+	{
+		tmp = cur_chunk->next;
+		cur_chunk->next = *scrap_list;
+		*scrap_list = cur_chunk;
+		cur_chunk = tmp;
+		chunk_idx = 0;
+		if(!cur_chunk)
+			return NULL;
+	}
+
+	return victim;
+}
+
+
+static inline size_t count_elements_in_list(struct seplist_chunk *l)
+{
+	size_t cnt = 0, i;
+	while(l)
+	{
+		for(i = 0; i < SEPLIST_CHUNK_ENTRIES; i++)
+			if(seplist_entry_has_entry(l->obj[i]))
+				cnt++;
+		l = l->next;
+	}
+	return cnt;
+}
+
+
+static inline void finalize_submerge(struct seplist_sort_scrap_buf* buf,
+		struct seplist_chunk **scrapped, struct seplist_head** remainder,
+		struct seplist_chunk **tail)
+{
+	struct seplist_chunk *rchunk;
+	while(buf->fill && *scrapped)
+	{
+		*tail = pop_buf_into_scrapped(buf, scrapped);
+		tail = &(*tail)->next;
+	}
+
+	if(buf->fill)
+	{
+		/* It could be that there is still a remainder in scrap_buf, in
+		 * that case fill it up with remainder list*/
+		while(!*scrapped)
+		{
+			insert_obj_into_scrap_buf(buf, *remainder);
+			remainder = __seplist_sort_next_sepentry(remainder, scrapped);
+		}
+		*tail = pop_buf_into_scrapped(buf, scrapped);
+		tail = &(*tail)->next;
+		BUG_ON(buf->fill);
+	}
+
+	/* Now we add the remainder to the tail */
+	rchunk = seplist_meta_to_chunk(remainder);
+	/* Set all previous pointers to 0 */
+	memset(rchunk->obj, 0, ((void **)remainder - rchunk->obj) * sizeof(struct seplist_head *));
+
+	*tail = rchunk;
+}
+
+
+static inline void add_chunk_to_tmp_list(struct seplist_chunk **head,
+		struct seplist_chunk **tail, struct seplist_chunk *ele)
+{
+	struct seplist_chunk *prev;
+	if(unlikely(!*head))
+		*head = *tail = ele;
+	else
+	{
+		prev = *tail;
+		prev->next = ele;
+		ele->prev = prev;
+		*tail = ele;
+	}
+}
+
+static inline void finalize_submerge_final(void *priv, cmp_func cmp, struct seplist_sort_scrap_buf* buf,
+		struct seplist_chunk **scrapped, struct seplist_head** remainder,
+		struct seplist_chunk **head, struct seplist_chunk **tail)
+{
+	struct seplist_chunk *rchunk, *tmp;
+	struct seplist_head *dummy;
+	u8 cnt = 0;
+	while(buf->fill && *scrapped)
+	{
+		tmp = pop_buf_into_scrapped(buf, scrapped);
+		fix_double_linking(tmp);
+		add_chunk_to_tmp_list(head, tail, tmp);
+	}
+
+	if(buf->fill)
+	{
+		/* It could be that there is still a remainder in scrap_buf, in
+		 * that case fill it up with remainder list*/
+		while(!*scrapped)
+		{
+			insert_obj_into_scrap_buf(buf, *remainder);
+			remainder = __seplist_sort_next_sepentry(remainder, scrapped);
+		}
+		tmp = pop_buf_into_scrapped(buf, scrapped);
+		fix_double_linking(tmp);
+		add_chunk_to_tmp_list(head, tail, tmp);
+		BUG_ON(buf->fill);
+	}
+
+
+	if(remainder)
+	{
+		/* Now we add the remainder to the tail */
+		rchunk = seplist_meta_to_chunk(remainder);
+
+		/* Set all previous pointers to 0 */
+		memset(rchunk->obj, 0, ((void **)remainder - rchunk->obj) * sizeof(struct seplist_head *));
+
+		fix_double_linking(rchunk);
+		add_chunk_to_tmp_list(head, tail, rchunk);
+
+		/* Set the previous ptrs of remainder */
+		tmp = rchunk->next;
+		while(tmp)
+		{
+			/*
+			 * If the merge is highly unbalanced (e.g. the input is
+			 * already sorted), this loop may run many iterations.
+			 * Continue callbacks to the client even though no
+			 * element comparison is needed, so the client's cmp()
+			 * routine can invoke cond_resched() periodically.
+			 */
+			if(unlikely(!++cnt))
+			{
+				dummy = *(struct seplist_head **)seplist_get_next_used_spot_in_chunk(rchunk, 0);
+				BUG_ON(!dummy);
+				cmp(priv, dummy, dummy);
+			}
+
+			fix_double_linking(tmp);
+			tmp->prev = rchunk;
+			rchunk = tmp;
+			tmp = tmp->next;
+		}
+	}
+}
+
+
+/*
+ * Returns a list organized in an intermediate format suited
+ * to chaining of merge() calls: null-terminated, no reserved or
+ * sentinel head node, "prev" links not maintained.
+ *
+ *
+ * Merging will go the following way:
+ *
+ * We go through the elements of the two lists as usual and insert the elements
+ * sorted in the scrapbuf. Whenever we're done with a chunk, this chunk will be
+ * inserted into the scrapped list. Whenever we've added enough elements
+ * (SEPLIST_CHUNK_ENTRIES) in the scrap_buf and we have a chunk in the scraplist,
+ * we will copy SEPLIST_CHUNK_ENTRIES into the head of the scraplist and add
+ * it to the output list
+ *
+ */
+__attribute__((nonnull(2,3,4)))
+static struct seplist_chunk *merge(void *priv, cmp_func cmp,
+				struct seplist_chunk *a, struct seplist_chunk *b)
+{
+	struct seplist_chunk *head, **tail = &head;
+
+	struct seplist_head **cura;
+	struct seplist_head **curb;
+
+
+	/* Working scrap buf */
+	struct seplist_sort_scrap_buf scrap_buf;
+	struct seplist_chunk *scrapped = NULL;
+	scrap_buf.fill = 0;
+
+	/* Initialize iterator */
+	cura = (struct seplist_head **)seplist_get_next_used_spot_in_chunk(a, 0);
+	curb = (struct seplist_head **)seplist_get_next_used_spot_in_chunk(b, 0);
+
+	for (;;) {
+		/* if equal, take 'a' -- important for sort stability */
+		if (cmp(priv, *cura, *curb) <= 0) {
+         insert_obj_into_scrap_buf(&scrap_buf, *cura);
+         cura = __seplist_sort_next_sepentry(cura, &scrapped);
+
+			if (!cura) {
+				finalize_submerge(&scrap_buf, &scrapped, curb, tail);
+				break;
+			}
+		} else {
+         insert_obj_into_scrap_buf(&scrap_buf, *curb);
+         curb = __seplist_sort_next_sepentry(curb, &scrapped);
+			if (!curb) {
+				finalize_submerge(&scrap_buf, &scrapped, cura, tail);
+				break;
+			}
+		}
+		if(scrap_buf.fill >= SEPLIST_CHUNK_ENTRIES && scrapped)
+		{
+			*tail = pop_buf_into_scrapped(&scrap_buf, &scrapped);
+			tail = &(*tail)->next;
+		}
+	}
+
+
+	/* Free unused chunks */
+	while(scrapped)
+	{
+		struct seplist_chunk *tmp = scrapped->next;
+		__seplist_free_chunk(scrapped);
+		scrapped = tmp;
+	}
+	return head;
+}
+
+/*
+ * Combine final list merge with restoration of standard doubly-linked
+ * list structure.  This approach duplicates code from merge(), but
+ * runs faster than the tidier alternatives of either a separate final
+ * prev-link restoration pass, or maintaining the prev links
+ * throughout.
+ */
+__attribute__((nonnull(2,3,5,6)))
+static void merge_final(void *priv, cmp_func cmp, struct seplist_head *head_h,
+			int head_at_begin,
+			struct seplist_chunk *a, struct seplist_chunk *b)
+{
+	struct seplist_chunk *head = NULL, *tail = NULL;
+
+	struct seplist_head **cura, **curb;
+
+	/* Working scrap buf */
+	struct seplist_sort_scrap_buf scrap_buf;
+	struct seplist_chunk *scrapped = NULL;
+
+	scrap_buf.fill = 0;
+
+	/* Initialize iterator */
+	cura = (struct seplist_head **)seplist_get_next_used_spot_in_chunk(a, 0);
+	curb = (struct seplist_head **)seplist_get_next_used_spot_in_chunk(b, 0);
+
+	/* If we need to insert the head at the beginning, we insert it first
+	 * in the scrap_buf. Note, however, that now if the list was fully packed,
+	 * we would end up with not enough chunks. Therefore, we add a new chunk
+	 * into scrapped (even though it will likely be freed) */
+	if(head_at_begin)
+	{
+		insert_obj_into_scrap_buf(&scrap_buf, head_h);
+		scrapped = new_seplist_chunk();
+		scrapped->next = NULL;
+	}
+
+	for (;;) {
+		/* if equal, take 'a' -- important for sort stability */
+		if (cmp(priv, *cura, *curb) <= 0) {
+         insert_obj_into_scrap_buf(&scrap_buf, *cura);
+         cura = __seplist_sort_next_sepentry(cura, &scrapped);
+
+			if (!cura) {
+				finalize_submerge_final(priv, cmp, &scrap_buf, &scrapped,
+						curb, &head, &tail);
+				break;
+			}
+		} else {
+         insert_obj_into_scrap_buf(&scrap_buf, *curb);
+         curb = __seplist_sort_next_sepentry(curb, &scrapped);
+			if (!curb) {
+				finalize_submerge_final(priv, cmp, &scrap_buf, &scrapped,
+						cura, &head, &tail);
+				break;
+			}
+		}
+		if(scrap_buf.fill >= SEPLIST_CHUNK_ENTRIES && scrapped)
+		{
+			struct seplist_chunk *new = pop_buf_into_scrapped(&scrap_buf, &scrapped);
+			fix_double_linking(new);
+			add_chunk_to_tmp_list(&head, &tail, new);
+		}
+	}
+
+	/* Add head if not at begin and set final links */
+	if(!head_at_begin) {
+		struct seplist_chunk *tailchunk = tail, *new;
+
+		/* If head doesn't fit anymore, allocate a new chunk */
+		if(unlikely(seplist_entry_has_entry(tailchunk->obj[SEPLIST_CHUNK_ENTRIES-1])))
+		{
+			new = new_seplist_chunk();
+			add_chunk_to_tmp_list(&head, &tail, new);
+			new->obj[SEPLIST_CHUNK_ENTRIES-1] = head_h;
+			head_h->meta = &new->obj[SEPLIST_CHUNK_ENTRIES-1];
+		}
+	}
+
+	tail->next = head;
+	head->prev = tail;
+
+	/* Free unused chunks */
+	while(scrapped)
+	{
+		struct seplist_chunk *tmp = scrapped->next;
+		__seplist_free_chunk(scrapped);
+		scrapped = tmp;
+	}
+}
+
+/**
+ * seplist_sort - sort a list
+ * @priv: private data, opaque to list_sort(), passed to @cmp
+ * @head: the list to sort
+ * @cmp: the elements comparison function
+ *
+ * The comparison funtion @cmp must return > 0 if @a should sort after
+ * @b ("@a > @b" if you want an ascending sort), and <= 0 if @a should
+ * sort before @b *or* their original order should be preserved.  It is
+ * always called with the element that came first in the input in @a,
+ * and list_sort is a stable sort, so it is not necessary to distinguish
+ * the @a < @b and @a == @b cases.
+ *
+ * This is compatible with two styles of @cmp function:
+ * - The traditional style which returns <0 / =0 / >0, or
+ * - Returning a boolean 0/1.
+ * The latter offers a chance to save a few cycles in the comparison
+ * (which is used by e.g. plug_ctx_cmp() in block/blk-mq.c).
+ *
+ * A good way to write a multi-word comparison is::
+ *
+ *	if (a->high != b->high)
+ *		return a->high > b->high;
+ *	if (a->middle != b->middle)
+ *		return a->middle > b->middle;
+ *	return a->low > b->low;
+ *
+ *
+ * This mergesort is as eager as possible while always performing at least
+ * 2:1 balanced merges.  Given two pending sublists of size 2^k, they are
+ * merged to a size-2^(k+1) list as soon as we have 2^k following elements.
+ *
+ * Thus, it will avoid cache thrashing as long as 3*2^k elements can
+ * fit into the cache.  Not quite as good as a fully-eager bottom-up
+ * mergesort, but it does use 0.2*n fewer comparisons, so is faster in
+ * the common case that everything fits into L1.
+ *
+ *
+ * The merging is controlled by "count", the number of elements in the
+ * pending lists.  This is beautiully simple code, but rather subtle.
+ *
+ * Each time we increment "count", we set one bit (bit k) and clear
+ * bits k-1 .. 0.  Each time this happens (except the very first time
+ * for each bit, when count increments to 2^k), we merge two lists of
+ * size 2^k into one list of size 2^(k+1).
+ *
+ * This merge happens exactly when the count reaches an odd multiple of
+ * 2^k, which is when we have 2^k elements pending in smaller lists,
+ * so it's safe to merge away two lists of size 2^k.
+ *
+ * After this happens twice, we have created two lists of size 2^(k+1),
+ * which will be merged into a list of size 2^(k+2) before we create
+ * a third list of size 2^(k+1), so there are never more than two pending.
+ *
+ * The number of pending lists of size 2^k is determined by the
+ * state of bit k of "count" plus two extra pieces of information:
+ *
+ * - The state of bit k-1 (when k == 0, consider bit -1 always set), and
+ * - Whether the higher-order bits are zero or non-zero (i.e.
+ *   is count >= 2^(k+1)).
+ *
+ * There are six states we distinguish.  "x" represents some arbitrary
+ * bits, and "y" represents some arbitrary non-zero bits:
+ * 0:  00x: 0 pending of size 2^k;           x pending of sizes < 2^k
+ * 1:  01x: 0 pending of size 2^k; 2^(k-1) + x pending of sizes < 2^k
+ * 2: x10x: 0 pending of size 2^k; 2^k     + x pending of sizes < 2^k
+ * 3: x11x: 1 pending of size 2^k; 2^(k-1) + x pending of sizes < 2^k
+ * 4: y00x: 1 pending of size 2^k; 2^k     + x pending of sizes < 2^k
+ * 5: y01x: 2 pending of size 2^k; 2^(k-1) + x pending of sizes < 2^k
+ * (merge and loop back to state 2)
+ *
+ * We gain lists of size 2^k in the 2->3 and 4->5 transitions (because
+ * bit k-1 is set while the more significant bits are non-zero) and
+ * merge them away in the 5->2 transition.  Note in particular that just
+ * before the 5->2 transition, all lower-order bits are 11 (state 3),
+ * so there is one list of each smaller size.
+ *
+ * When we reach the end of the input, we merge all the pending
+ * lists, from smallest to largest.  If you work through cases 2 to
+ * 5 above, you can see that the number of elements we merge with a list
+ * of size 2^k varies from 2^(k-1) (cases 3 and 5 when x == 0) to
+ * 2^(k+1) - 1 (second merge of case 5 when x == 2^(k-1) - 1).
+ */
+__attribute__((nonnull(2,3)))
+void seplist_sort(void *priv, struct seplist_head *head_h,
+		int (*cmp)(void *priv, struct seplist_head *a,
+			struct seplist_head *b))
+{
+#ifdef CONFIG_SEPLIST_KSMAP
+	ksmap_disable_smap();
+#endif
+	struct seplist_chunk *headchunk, *list, *pending = NULL;
+	struct seplist_head **head_m = (struct seplist_head **)head_h->meta;
+	size_t count = 0;	/* Count of pending */
+	size_t cidx;
+	int head_at_begin;
+
+	if(!head_m)
+		goto out;
+
+
+	if (seplist_next(head_h) == seplist_prev(head_h))	/* Zero or one elements */
+		goto out;
+
+
+	/*
+	 * Convert to a null-terminated singly-linked list without head entry.
+	 * To do this we have to:
+	 *  - if head is front or back of a chunk delete it
+	 *  - if head is in middle, split the chunk and then delete head
+	 * At the end, we then need to reinsert head
+	 * */
+	headchunk = seplist_meta_to_chunk(head_m);
+	cidx = seplist_meta_to_chunkidx(head_m);
+
+	/* If only one element, just sort it */
+	if(headchunk->next == headchunk)
+	{
+		head_at_begin = cidx == 0;
+		*head_m = NULL;
+		sort_single_chunk(priv, (cmp_func)cmp, headchunk);
+
+		if (head_at_begin) {
+			struct seplist_head *tmp, *cur, **i;
+			/* Move all entries by one and insert head at beginning */
+			for(cur = head_h, i = (struct seplist_head **)&headchunk->obj[0];cur;i++)
+			{
+				tmp = *i;
+				*i = cur;
+				cur = tmp;
+			}
+		} else {
+			headchunk->obj[SEPLIST_CHUNK_ENTRIES-1] = head_h;
+		}
+		fix_double_linking(headchunk);
+		goto out;
+	}
+
+	if(cidx == 0)
+	{
+		head_at_begin = 1;
+		list = headchunk;
+		/* Delete entry for now */
+		*head_m = NULL;
+	}
+	else if(cidx == SEPLIST_CHUNK_ENTRIES - 1)
+	{
+		head_at_begin = 0;
+		list = headchunk->next;
+		/* Delete entry for now */
+		*head_m = NULL;
+	}
+	else
+	{
+		seplist_split_chunk((void**)head_m);
+		head_at_begin = 0;
+		list = headchunk->next;
+		/* Delete entry for now */
+		*head_m = NULL;
+	}
+
+
+	/* Make singly linked */
+	list->prev->next = NULL;
+	list->prev = NULL;
+
+	/*
+	 * Data structure invariants:
+	 * - All chunks are singly linked and null-terminated; prev
+	 *   pointers are not maintained.
+	 * - pending is a prev-linked "list of chunks" of sorted
+	 *   sublists awaiting further merging.
+	 * - Each of the sorted sublists is power-of-two in size.
+	 * - Sublists are sorted by size and age, smallest & newest at front.
+	 * - There are zero to two sublists of each size.
+	 * - A pair of pending sublists are merged as soon as the number
+	 *   of following pending elements equals their size (i.e.
+	 *   each time count reaches an odd multiple of that size).
+	 *   That ensures each later final merge will be at worst 2:1.
+	 * - Each round consists of:
+	 *   - Merging the two sublists selected by the highest bit
+	 *     which flips when count is incremented, and
+	 *   - Adding an element from the input as a size-1 sublist.
+	 */
+	do {
+		size_t bits;
+		struct seplist_chunk **tail = &pending;
+
+		/* Find the least-significant clear bit in count */
+		for (bits = count; bits & 1; bits >>= 1)
+			tail = &(*tail)->prev;
+		/* Do the indicated merge */
+		if (likely(bits)) {
+			struct seplist_chunk *a = *tail, *b = a->prev;
+
+			a = merge(priv, (cmp_func)cmp, b, a);
+			/* Install the merged result in place of the inputs */
+			a->prev = b->prev;
+			*tail = a;
+		}
+
+		/* Move one element from input list to pending */
+		list->prev = pending;
+		pending = list;
+		list = list->next;
+		pending->next = NULL;
+		sort_single_chunk(priv, (cmp_func)cmp, pending);
+		count++;
+	} while (list);
+
+	/* End of input; merge together all the pending lists. */
+	list = pending;
+	pending = pending->prev;
+	for (;;) {
+		struct seplist_chunk *next = pending->prev;
+
+		if (!next)
+			break;
+		list = merge(priv, (cmp_func)cmp, pending, list);
+		pending = next;
+	}
+
+	/* The final merge, rebuilding prev links */
+	merge_final(priv, (cmp_func)cmp, head_h, head_at_begin, pending, list);
+out:
+#ifdef CONFIG_SEPLIST_KSMAP
+	ksmap_enable_smap();
+#endif
+	return;
+}
+EXPORT_SYMBOL(seplist_sort);
+
+
+
+#endif

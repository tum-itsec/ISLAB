diff --git a/lib/seplist_sort-test.c b/lib/seplist_sort-test.c
new file mode 100644
index 000000000000..e69ccad5949c
--- /dev/null
+++ b/lib/seplist_sort-test.c
@@ -0,0 +1,220 @@
+// SPDX-License-Identifier: GPL-2.0-only
+
+#include <kunit/test.h>
+#include <linux/kernel.h>
+#include <linux/seplist_sort.h>
+#include <linux/seplist.h>
+#include <linux/module.h>
+#include <linux/printk.h>
+#include <linux/slab.h>
+#include <linux/random.h>
+
+/*
+ * The pattern of set bits in the list length determines which cases
+ * are hit in seplist_sort().
+ */
+
+#define TEST_LIST_LEN_SMALL (3) /* not including head */
+#define TEST_LIST_LEN_MEDIUM (20) /* not including head */
+#define TEST_LIST_LEN_BIG (512+128+2) /* not including head */
+
+#define TEST_POISON1 0xDEADBEEF
+#define TEST_POISON2 0xA324354C
+
+struct debug_el {
+	unsigned int poison1;
+	struct seplist_head list;
+	unsigned int poison2;
+	int value;
+	unsigned serial;
+};
+
+struct test_info {
+	struct kunit *test;
+	struct debug_el **elts;
+	unsigned len;
+};
+
+static void check(struct test_info *testi, struct debug_el *ela, struct debug_el *elb)
+{
+	struct kunit *test = testi->test;
+
+	KUNIT_EXPECT_LE(test, ela->serial, testi->len);
+	KUNIT_EXPECT_LE(test, elb->serial, testi->len);
+	KUNIT_EXPECT_PTR_EQ(test, testi->elts[ela->serial], ela);
+	KUNIT_EXPECT_PTR_EQ(test, testi->elts[elb->serial], elb);
+	KUNIT_EXPECT_EQ(test, ela->poison1, TEST_POISON1);
+	KUNIT_EXPECT_EQ(test, ela->poison2, TEST_POISON2);
+	KUNIT_EXPECT_EQ(test, elb->poison1, TEST_POISON1);
+	KUNIT_EXPECT_EQ(test, elb->poison2, TEST_POISON2);
+}
+
+static int cmp(void *priv, struct seplist_head *a, struct seplist_head *b)
+{
+	struct test_info *testi = priv;
+	struct debug_el *ela, *elb;
+
+	KUNIT_EXPECT_TRUE(testi->test, a);
+	KUNIT_EXPECT_TRUE(testi->test, b);
+
+	ela = container_of(a, struct debug_el, list);
+	elb = container_of(b, struct debug_el, list);
+
+	check(testi, ela, elb);
+	return ela->value - elb->value;
+}
+
+static void seplist_test_check_list(struct test_info *testi,
+		struct seplist_head *head)
+{
+	struct seplist_head *cur;
+	struct debug_el *el, *el1;
+	unsigned count = seplist_empty(head) ? 0 : 1;
+	struct kunit *test = testi->test;
+	for (cur = seplist_next(head); seplist_next(cur) != head; cur = seplist_next_unchecked(cur)) {
+		int cmp_result;
+
+		KUNIT_EXPECT_PTR_EQ(test, seplist_prev(seplist_next(cur)), cur);
+
+		cmp_result = cmp(testi, cur, seplist_next(cur));
+		KUNIT_EXPECT_TRUE(test, cmp_result <= 0);
+
+
+		el = container_of(cur, struct debug_el, list);
+		el1 = container_of(seplist_next(cur), struct debug_el, list);
+		KUNIT_EXPECT_TRUE(test, !cmp_result == 0 || el->serial < el1->serial);
+
+		check(testi, el, el1);
+		count++;
+	}
+
+
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(head), cur);
+	KUNIT_EXPECT_EQ(test, count, testi->len);
+}
+
+static struct debug_el **create_seplist_list(struct kunit *test,
+		struct seplist_head *head, size_t len)
+{
+	struct debug_el **elts, *el;
+	size_t i;
+	elts = kcalloc(len, sizeof(*elts), GFP_KERNEL);
+	KUNIT_EXPECT_TRUE(test, elts);
+
+	for (i = 0; i < len; i++) {
+		el = kmalloc(sizeof(*el), GFP_KERNEL);
+		KUNIT_EXPECT_TRUE(test, el);
+
+		 /* force some equivalencies */
+		el->value = prandom_u32() % max(len / 3, (size_t)2);
+		el->serial = i;
+		el->poison1 = TEST_POISON1;
+		el->poison2 = TEST_POISON2;
+		elts[i] = el;
+		seplist_add_tail(&el->list, head);
+	}
+
+	return elts;
+}
+
+static void free_seplist_list(struct test_info *testi)
+{
+	size_t i;
+	for (i = 0; i < testi->len; i++)
+		kfree(testi->elts[i]);
+	kfree(testi->elts);
+}
+
+static void seplist_sort_test_empty(struct kunit *test)
+{
+	SEPLIST_HEAD(head);
+	struct test_info testi = { .test = test, .elts = NULL, .len = 0 };
+
+	seplist_sort(&testi, &head, cmp);
+	seplist_test_check_list(&testi, &head);
+}
+

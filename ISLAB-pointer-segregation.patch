diff --git a/mm/metaslab.c b/mm/metaslab.c
new file mode 100644
index 000000000000..d8e262e77b56
--- /dev/null
+++ b/mm/metaslab.c
@@ -0,0 +1,385 @@
+#include <linux/types.h>
+#include <linux/mm_types.h>
+#include <linux/mm.h>
+#include <linux/metaslab.h>
+#include <linux/slab.h>
+#include <linux/slub_def.h>
+
+#include <xen/interface/altp2m.h>
+
+#include <asm/xen/page.h>
+
+#include <asm/percpu.h>
+
+#include "slab.h"
+
+DEFINE_PER_CPU(struct list_head *, pcpu_metapages_empty);
+
+DEFINE_PER_CPU(struct list_head *, pcpu_sizes_metaslabs_partial);
+
+struct large_size_metaslab {
+	unsigned int size;
+	struct list_head head_metaslab_partial;
+	struct list_head entry_metaslabs_partial; 
+};
+static struct kmem_cache *cache_large_size_metaslab;
+
+DEFINE_PER_CPU(struct list_head *, pcpu_large_sizes_metaslabs_partial);
+
+struct list_head *misc_data_page = NULL;
+
+#define MS_CHUNK_NUM_METAPAGES 64
+
+static void ms_fuel_up_metapages(unsigned int n) {
+	int rc, i;
+	unsigned long physical_metaspace_base = page_to_pfn(alloc_pages(__GFP_ZERO, get_order(n * PAGE_SIZE)));	
+	struct page *iter;
+	struct list_head *metapages_empty = this_cpu_read_stable(pcpu_metapages_empty);
+
+	ms_printk("Allocated %d phyiscial metapages at gfn %lx", n, physical_metaspace_base);
+
+	for (i = 0; i < n; i++) {
+		iter = pfn_to_page(physical_metaspace_base + i);
+
+		iter->available = 512;
+	
+		rc = altp2m_set_mem_access(dataview, physical_metaspace_base + i, XENMEM_access_n);
+		if (rc) {
+			ms_printk("Error setting mem access of %lx in view %d (dataview): %d", physical_metaspace_base + i, dataview, rc);
+			BUG_ON(true);
+		}
+		
+		rc = altp2m_set_suppress_ve(dataview, physical_metaspace_base + i, false);
+		if (rc) {
+			ms_printk("Error setting suppress VE bit of %lx in view %d (dataview): %d", physical_metaspace_base + i, dataview, rc);
+			BUG_ON(true);
+		}
+		
+		rc = altp2m_set_suppress_ve(metaview, physical_metaspace_base + i, false);
+		if (rc) {
+			ms_printk("Error setting suppress VE bit of %lx in view %d (metaview): %d", physical_metaspace_base + i, dataview, rc);
+			BUG_ON(true);
+		}
+		
+		INIT_LIST_HEAD(&iter->metapages_list);	
+		list_add(&iter->metapages_list, metapages_empty);
+	}
+}
+
+static void __init ms_init_cpu(void *info)
+{
+	int rc, i = smp_processor_id();
+	struct list_head *sizes_metaslabs_partial = (struct list_head *)page_to_virt(alloc_pages(GFP_KERNEL | __GFP_ZERO, 1)), *large_sizes_metaslabs_partial, *metapages_empty;
+   
+	if (!misc_data_page)
+		misc_data_page = (struct list_head *)(page_to_virt(alloc_page(GFP_KERNEL | __GFP_ZERO)));
+	large_sizes_metaslabs_partial = &misc_data_page[i * 2];
+	metapages_empty = &misc_data_page[i * 2 + 1];
+
+	for (i = 0; i < 512; i++)
+		INIT_LIST_HEAD(&sizes_metaslabs_partial[i]);
+	this_cpu_write(pcpu_sizes_metaslabs_partial, sizes_metaslabs_partial);
+
+	INIT_LIST_HEAD(large_sizes_metaslabs_partial);
+	this_cpu_write(pcpu_large_sizes_metaslabs_partial, large_sizes_metaslabs_partial);
+
+	INIT_LIST_HEAD(metapages_empty);
+	this_cpu_write(pcpu_metapages_empty, metapages_empty);
+	
+	ms_fuel_up_metapages(MS_CHUNK_NUM_METAPAGES);
+
+	rc = vmfunc(dataview);
+	if (rc) {
+		printk("[MS] Error vmfunc-ing into dataview: %d", rc);
+		BUG_ON(true);
+	}
+
+	printk("[MS] Initialized CPU %d", smp_processor_id());
+}
+
+void __init ms_init_metaslab(void) {
+	int rc;
+
+	rc = altp2m_create_view(XENMEM_access_rwx, &dataview);
+	if (rc) {
+		ms_printk("Error creating the dataview: %d", rc);
+		BUG_ON(true);
+	}
+	
+	rc = altp2m_create_view(XENMEM_access_rwx, &metaview);
+	if (rc) {
+		ms_printk("Error creating the metaview: %d", rc);
+		BUG_ON(true);
+	}
+
+	printk("[MS] Metaslab is online");
+	
+	ms_init_cpu(NULL);
+}
+
+void __init ms_init_smp(void)                              
+{                                                                     
+	struct cpumask cpumask_except_self;                               
+
+	cpumask_copy(&cpumask_except_self, cpu_online_mask);              
+
+	cpumask_clear_cpu(smp_processor_id(), &cpumask_except_self);      
+
+	on_each_cpu_mask(&cpumask_except_self, ms_init_cpu, NULL, 1);
+}                                                                     
+
+static inline void list_cut_n(struct list_head *list,
+		struct list_head *head,
+		unsigned long n)
+{
+	struct list_head *last = head->next;	
+	
+	while (--n > 0) {
+		last = last->next;
+	}
+	
+	list_cut_position(list, head, last);
+}
+
+static inline uint16_t list_size(struct list_head *list)
+{
+	struct list_head *tmp;
+	uint16_t count = 0;
+
+	list_for_each(tmp, list)
+		count++;
+
+	return count;
+}
+
+void ms_map_to_metaslab(struct kmem_cache *s, struct page *page, unsigned int order) {
+
+	bool init_metaslab_head;
+	int rc, i;
+	unsigned long gfn, no_pages, no_objects, objs_per_metaobj, flags, *metaslab_head_va, size;
+	struct page *metapage, *metaslab_head;
+	struct list_head *size_metaslabs;
+	LIST_HEAD(chosen_metaslab);
+
+#ifdef CONFIG_SLUB_HARDENED_FREELIST_METASLAB_SELECTIVE
+	if (!(s->flags & SLAB_ISOLATE_FP))
+		return;
+#endif
+
+	if (!ms_should_isolate_kmem_cache(s))
+		return;
+
+	local_irq_save(flags);
+
+	gfn = page_to_pfn(page);
+
+	no_pages = 1 << order;
+
+	no_objects = page->objects;
+
+	size = s->size;
+
+	ms_printk("Hardening kmem_cache of size %d, s->name = %s, order = %d, no_objects = %d", s->size, s->name, order, no_objects);
+
+	if (likely(size < 4097)) {
+		struct list_head *sizes_metaslabs_partial;
+		sizes_metaslabs_partial = this_cpu_read_stable(pcpu_sizes_metaslabs_partial);
+		size_metaslabs = &sizes_metaslabs_partial[(size >> 3) - 1];
+	}
+	else {
+		struct list_head *iter;
+		struct large_size_metaslab *curr_large_size_metaslab = NULL;
+		struct list_head *large_sizes_metaslabs_partial = this_cpu_read_stable(pcpu_large_sizes_metaslabs_partial);
+
+		list_for_each(iter, large_sizes_metaslabs_partial) {
+			curr_large_size_metaslab = list_entry(iter, struct large_size_metaslab, entry_metaslabs_partial);
+
+			if (curr_large_size_metaslab->size == size)
+				break;
+		}
+
+		if (iter == large_sizes_metaslabs_partial) {
+
+			curr_large_size_metaslab = kmalloc(sizeof(struct large_size_metaslab), __GFP_ZERO);
+
+			INIT_LIST_HEAD(&curr_large_size_metaslab->head_metaslab_partial);
+			INIT_LIST_HEAD(&curr_large_size_metaslab->entry_metaslabs_partial);
+
+			curr_large_size_metaslab->size = size;
+
+			list_add_tail(&curr_large_size_metaslab->entry_metaslabs_partial, large_sizes_metaslabs_partial);
+		}
+
+		size_metaslabs = &curr_large_size_metaslab->head_metaslab_partial;
+	}
+
+	if (list_empty(size_metaslabs)) {
+		struct list_head *metapages_empty = this_cpu_read_stable(pcpu_metapages_empty);
+
+		if (list_size(metapages_empty) < no_pages) {
+			ms_printk("Ran out of physical metapages: left %d, needed %d", list_size(metapages_empty), no_pages);
+			ms_fuel_up_metapages(MS_CHUNK_NUM_METAPAGES);
+		}
+		list_cut_n(&chosen_metaslab, metapages_empty, no_pages);
+		init_metaslab_head = true;
+	} else {
+		list_cut_n(&chosen_metaslab, size_metaslabs, no_pages);
+		init_metaslab_head = false;
+	}
+
+	metaslab_head = list_first_entry(&chosen_metaslab, struct page, metapages_list);
+
+	metapage = metaslab_head;
+	do {
+		rc = altp2m_change_gfn(metaview, gfn, page_to_pfn(metapage));
+		if (rc) {
+			printk("Error changing gfn %lx to %lx in view %d (metaview): %d",
+					gfn, page_to_pfn(metapage), metaview, rc);
+			WARN_ON(true);
+		}
+		metapage = list_next_entry(metapage, metapages_list);
+		gfn++;
+	} while (--no_pages > 0);
+
+	page->metapage_pfn = page_to_pfn(metaslab_head);
+
+	metaslab_head_va = page_to_virt(metaslab_head);
+
+	vmfunc(metaview);
+
+	if (init_metaslab_head) {
+		objs_per_metaobj = (((size >> 3) - 1) % 512) + 1;
+
+		for (i = 0; i < objs_per_metaobj - 1; i++)
+			metaslab_head_va[i] = i + 1;
+
+		page->metapage_off = 0;
+		metaslab_head->curr_offset = 1;
+
+		metaslab_head->available = objs_per_metaobj;
+	}
+	else {
+		page->metapage_off = metaslab_head->curr_offset;
+		metaslab_head->curr_offset = metaslab_head_va[metaslab_head->curr_offset];
+	}
+
+	vmfunc(dataview);
+
+	metaslab_head->available--;
+
+	if (metaslab_head->available) {
+		// store metaslab in the list of metaslabs for the size
+		list_splice(&chosen_metaslab, size_metaslabs);
+	} else {
+		// remove the head, the metapages themselves will form a circular list
+		list_del(&chosen_metaslab);
+	}
+
+	local_irq_restore(flags);
+
+	ms_printk("Finished hardening kmem_cache %s of size %d; page->metapage_pfn = %llx, page->metapage_off = %llx, available = %d", s->name, s->size, page->metapage_pfn, page->metapage_off, metaslab_head->available);
+}
+
+void ms_reclaim_metapages(struct kmem_cache *s, struct page *page, unsigned int order) {
+	int rc;
+	unsigned long gfn, old_available, no_pages, *metaslab_head_va, flags,
+				  fps_per_metaobject;
+	struct page *metaslab_head;
+	struct list_head *size_metaslabs;
+	LIST_HEAD(metaslab);
+
+#ifdef CONFIG_SLUB_HARDENED_FREELIST_METASLAB_SELECTIVE
+	if (!(s->flags & SLAB_ISOLATE_FP))
+		return;
+#endif
+
+	if (!ms_should_isolate_kmem_cache(s))
+		return;
+	
+	local_irq_save(flags);
+	
+	metaslab_head = pfn_to_page(page->metapage_pfn);
+
+	no_pages = 1 << order;
+
+	old_available = metaslab_head->available;
+
+	// one more twin slab slot available in the metaobject	
+	metaslab_head->available++;
+
+	ms_printk("Reclaiming metapages form kmem_cache of size %d, name = %s, new available = %d, no_pages = %d, curr_offset = %d", s->size, s->name, metaslab_head->available, no_pages, metaslab_head->curr_offset);
+
+	// get the metaslab head virtual address
+	metaslab_head_va = page_to_virt(metaslab_head);
+	
+	vmfunc(metaview);
+
+	metaslab_head_va[page->metapage_off] = metaslab_head->curr_offset;
+	
+	vmfunc(dataview);
+	
+	metaslab_head->curr_offset = page->metapage_off;
+
+	fps_per_metaobject = (((s->size >> 3) - 1) % 512) + 1;
+
+	if (metaslab_head->available != fps_per_metaobject && old_available == 0) {
+		// here add in partial lists
+		if (likely(s->size < 4097)) {
+			struct list_head *sizes_metaslabs_partial = this_cpu_read_stable(pcpu_sizes_metaslabs_partial);
+			size_metaslabs = &sizes_metaslabs_partial[(s->size >> 3) - 1];
+		}
+		else {
+			struct list_head *iter;
+			struct large_size_metaslab *curr_large_size_metaslab = NULL;
+			struct list_head *large_sizes_metaslabs_partial = this_cpu_read_stable(pcpu_large_sizes_metaslabs_partial);
+
+			list_for_each(iter, large_sizes_metaslabs_partial) {
+				curr_large_size_metaslab = list_entry(iter, struct large_size_metaslab, entry_metaslabs_partial);
+
+				if (curr_large_size_metaslab->size == s->size)
+					break;
+			}
+	
+			if (curr_large_size_metaslab)
+				size_metaslabs = &curr_large_size_metaslab->head_metaslab_partial;
+			else
+				BUG_ON(true);
+		}
+		
+		list_add_tail(&metaslab, &metaslab_head->metapages_list);
+		list_splice(&metaslab, size_metaslabs);	
+	} else if (metaslab_head->available == fps_per_metaobject && old_available != 0) {
+		struct list_head *metapages_empty = this_cpu_read_stable(pcpu_metapages_empty);
+		// here remove from partial lists and add in empty lists
+		list_cut_n(&metaslab, metaslab_head->metapages_list.prev, no_pages);
+		list_splice(&metaslab, metapages_empty);
+	}
+
+	gfn = page_to_pfn(page);
+	
+	while (no_pages-- > 0) {	
+		rc = altp2m_change_gfn(metaview, gfn, INVALID_GFN);
+		if (rc) {
+			ms_printk("Error changing gfn %lx to %lx in view %d (metaview): %d", gfn, INVALID_GFN, metaview, rc);
+			BUG_ON(true);
+		}
+		
+		rc = altp2m_set_suppress_ve(metaview, gfn, true);
+		if (rc) {
+			ms_printk("Error setting suppress VE bit of %lx in view %d (metaview): %d", gfn, metaview, rc);
+			BUG_ON(true);
+		}
+		gfn++;
+	}
+
+	page->metapage_pfn = 0x1ffffffff;
+	page->metapage_off = 0;
+
+	page_mapcount_reset(page);
+
+	init_page_count(page);
+
+	local_irq_restore(flags);
+	
+	ms_printk("Finished reclaiming metapages form kmem_cache of size %d", s->size);
+}

+/**
+ * Adds a new entry to the tail of the seplist in a lockless way, i.e.
+ * multiple CPUs are allowed to call this function concurrently.
+ *
+ * Beware: it is necessary to prevent any other modifications of the
+ *         existing seplist until all changes are completed, in other words
+ *         concurrent seplist_add_tail_lockless() calls should be protected
+ *         with a read lock, where write lock acts as a barrier which
+ *         makes sure all seplist_add_tail_lockless() calls are fully
+ *         completed.
+ *
+ *        Also an element can be locklessly added to the seplist only in one
+ *        direction i.e. either to the tail either to the head, otherwise
+ *        concurrent access will corrupt the seplist.
+ *
+ * Returns %false if element has been already added to the seplist, %true
+ * otherwise.
+ */
+static inline bool seplist_add_tail_lockless(struct seplist_head *new,
+					  struct seplist_head *head)
+{
+	struct seplist_chunk *newchunk, *headchunk, *prev;
+	struct seplist_head **head_m, **meta;
+	size_t cidx;
+	void **insert_loc;
+
+	head_m = (struct seplist_head **)READ_ONCE(head->meta);
+
+	smapdisable();
+
+	if(!head_m)
+	{
+		newchunk = new_seplist_chunk_init();
+		newchunk->obj[0] = &head->meta;
+		meta = (struct seplist_head **)&newchunk->obj[0];
+		/*
+		 * This is simple 'head->meta = newchunk' operation, but cmpxchg()
+		 * is used in order to detect that the head hasn't been just
+		 * allocated from another CPU: the winner observes
+		 * head->meta == NULL.
+		 */
+		if (cmpxchg(&head->meta, NULL, (void*)meta) != NULL)
+		{
+			/* On fail, read the headchunk and free previously allocated chunk */
+			__seplist_free_chunk(newchunk);
+			head_m = (struct seplist_head **)READ_ONCE(head->meta);
+		}
+		else
+			head_m = meta;
+	}
+
+
+check_head:
+	headchunk = seplist_meta_to_chunk(head_m);
+	cidx = seplist_meta_to_chunkidx(head_m);
+
+	/* We usually assume this function is only called with head at the beginning
+	 * (i.e. an add_tail list) if we do not have one, make it one */
+	if(unlikely(cidx && !seplist_is_head_first_in_chunk((void **)head_m))) {
+		/* If head is last, create new chunk and move head first */
+		//if (seplist_is_head_last_in_chunk((void **)head_m))
+		//{
+			struct seplist_chunk *next;
+			newchunk = new_seplist_chunk();
+			newchunk->obj[0] = &head->meta;
+			next = headchunk->next;
+			newchunk->next = next;
+			newchunk->prev = headchunk;
+			meta = (struct seplist_head **)&newchunk->obj[0];
+
+			/*
+			 * This is simple 'head->meta = newchunk' operation, but cmpxchg()
+			 * is used in order to detect that the head hasn't been just
+			 * modified from another CPU: the winner observes
+			 * head->meta == head_m.
+			 */
+			if (cmpxchg(&head->meta, (void *)head_m, (void *)meta) != (void *)head_m)
+			{
+				/* On fail, read the headchunk and free previously allocated chunk */
+				__seplist_free_chunk(newchunk);
+				head_m = (struct seplist_head **)READ_ONCE(head->meta);
+				goto check_head;
+			}
+			else {
+				next->prev = newchunk;
+
+				/* Update headchunk->next if it wasn't updated by another CPU */
+				cmpxchg(&headchunk->next, next, newchunk);
+				/* This needs to happen after ->next is updated to make sure CPUs
+				 * currently iterating through the list will always be able to reach
+				 * the head element */
+				*head_m = NULL;
+				head_m = meta;
+			}
+		//}
+		//else
+		//{
+		//	BUG_ON(true);
+		//}
+	}
+
+	/* Get exclusive access on current element, i.e. detect if the element
+	 * already got added or is in the process thereof, winner should see
+	 * meta == NULL */
+	if (cmpxchg(&new->meta, NULL, (void*)1) != NULL) {
+		smapenable();
+		return false;
+	}
+
+retry:
+	/* Cannot find space in current chunk, look in prev one */
+	insert_loc = seplist_get_first_freespot_in_chunk(smp_load_acquire(&headchunk->prev), SEPLIST_CHUNK_ENTRIES - 1);
+
+	if (insert_loc)
+	{
+		/* Try to insert here */
+		if (cmpxchg(insert_loc, NULL, (void *)new) != NULL)
+			goto retry;
+
+		WRITE_ONCE(new->meta, insert_loc);
+	}
+	else
+	{
+		/* Insert new chunk atomically */
+		struct seplist_chunk *newchunk = new_seplist_chunk_init();
+		newchunk->obj[0] = new;
+		newchunk->next = headchunk;
+		smp_store_release(&new->meta, &newchunk->obj[0]);
+
+
+		/*
+		 * Initially ->next of a new element must be updated with the head
+		 * (we are inserting to the tail) and only then pointers are atomically
+		 * exchanged.  XCHG guarantees memory ordering, thus ->next should be
+		 * updated before pointers are actually swapped and pointers are
+		 * swapped before prev->next is updated.
+		 */
+		prev = xchg(&headchunk->prev, newchunk);
+
+		/*
+		 * It is safe to modify new->prev, because a new element
+		 * is added only to the tail and new->next is updated before XCHG.
+		 * Note, that because the head might've been moved we use xchg to synchronize
+		 * with the cmpxchg of headchunk->next
+		 */
+
+		xchg(&prev->next, newchunk);
+		newchunk->prev = prev;
+	}
+	smapenable();
+	return true;
+}
+
+
+static inline void seplist_del(struct seplist_head *entry)
+{
+	struct seplist_chunk *cur_chunk;
+	struct seplist_head **next;
+	void** meta;
+
+	if(!entry->meta)
+		return;
+
+	smapdisable();
+
+	seplist_check_integrity(entry);
+
+	meta = entry->meta;
+
+	entry->meta = NULL;
+	WRITE_ONCE(*meta, NULL);
+
+	next = seplist_next_sepentry(meta);
+	cur_chunk = seplist_meta_to_chunk(meta);
+	if(sepchunk_empty(cur_chunk))
+		__seplist_del_chunk(cur_chunk);
+
+	/* Delete empty heads to prevent memory leaks*/
+	if(seplist_next_sepentry((void **)next) == next)
+	{
+		cur_chunk = seplist_meta_to_chunk(next);
+		(*next)->meta = NULL;
+		__seplist_free_chunk(cur_chunk);
+	}
+	smapenable();
+
+}

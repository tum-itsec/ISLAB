diff --git a/lib/seplist-test.c b/lib/seplist-test.c
new file mode 100644
index 000000000000..36f701cbbeaa
--- /dev/null
+++ b/lib/seplist-test.c
@@ -0,0 +1,841 @@
+// SPDX-License-Identifier: GPL-2.0
+/*
+ * KUnit test for the Kernel Linked-seplist structures.
+ *
+ * Copyright (C) 2019, Google LLC.
+ * Author: David Gow <davidgow@google.com>
+ */
+#include <kunit/test.h>
+
+#include <linux/seplist.h>
+
+struct seplist_test_struct {
+	int data;
+	struct seplist_head seplist;
+};
+
+static void seplist_test_seplist_init(struct kunit *test)
+{
+	/* Test the different ways of initialising a seplist. */
+	struct seplist_head seplist1 = SEPLIST_HEAD_INIT(seplist1);
+	struct seplist_head seplist2;
+	SEPLIST_HEAD(seplist3);
+	struct seplist_head *seplist4;
+	struct seplist_head *seplist5;
+
+	INIT_SEPLIST_HEAD(&seplist2);
+
+	seplist4 = kzalloc(sizeof(*seplist4), GFP_KERNEL | __GFP_NOFAIL);
+	INIT_SEPLIST_HEAD(seplist4);
+
+	seplist5 = kmalloc(sizeof(*seplist5), GFP_KERNEL | __GFP_NOFAIL);
+	memset(seplist5, 0xFF, sizeof(*seplist5));
+	INIT_SEPLIST_HEAD(seplist5);
+
+	/* seplist_empty() checks both next and prev. */
+	KUNIT_EXPECT_TRUE(test, seplist_empty(&seplist1));
+	KUNIT_EXPECT_TRUE(test, seplist_empty(&seplist2));
+	KUNIT_EXPECT_TRUE(test, seplist_empty(&seplist3));
+	KUNIT_EXPECT_TRUE(test, seplist_empty(seplist4));
+	KUNIT_EXPECT_TRUE(test, seplist_empty(seplist5));
+
+	kfree(seplist4);
+	kfree(seplist5);
+}
+
+static void seplist_test_seplist_add(struct kunit *test)
+{
+	struct seplist_head a, b;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add(&a, &seplist);
+	seplist_add(&b, &seplist);
+
+	/* should be [seplist] -> b -> a */
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&seplist), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&b), &seplist);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&b), &a);
+}
+
+
+static void seplist_test_seplist_add_tail(struct kunit *test)
+{
+	struct seplist_head a, b;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add_tail(&a, &seplist);
+	seplist_add_tail(&b, &seplist);
+
+	/* should be [seplist] -> a -> b */
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&seplist), &a);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&seplist), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&a), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&a), &seplist);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&b), &seplist);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&b), &a);
+}
+
+
+
+static void seplist_test_seplist_add_many(struct kunit *test)
+{
+#define __NUM_ENTRIES (SEPLIST_CHUNK_ENTRIES*2)
+	struct seplist_head entries[__NUM_ENTRIES+1], *cur;
+	int i = 0;
+	INIT_SEPLIST_HEAD(&entries[0]);
+
+	for(i = __NUM_ENTRIES-1; i >= 0; i--)
+		seplist_add(&entries[i+1], &entries[0]);
+
+	i = 1;
+	seplist_for_each(cur, &entries[0]) {
+		KUNIT_EXPECT_PTR_EQ(test, cur, &entries[i]);
+		KUNIT_EXPECT_PTR_EQ(test, seplist_next(&entries[i]), &entries[(i+1) % (__NUM_ENTRIES + 1)]);
+		KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&entries[i]), &entries[i-1]);
+		i++;
+	}
+#undef __NUM_ENTRIES
+}
+
+static void seplist_test_seplist_add_tail_many(struct kunit *test)
+{
+#define __NUM_ENTRIES 3
+	struct seplist_head entries[__NUM_ENTRIES+1], *cur;
+	int i = 0;
+	INIT_SEPLIST_HEAD(&entries[0]);
+
+	for(i = 0; i < __NUM_ENTRIES; i++)
+		seplist_add_tail(&entries[i+1], &entries[0]);
+
+	i = 1;
+	seplist_for_each(cur, &entries[0]) {
+		KUNIT_EXPECT_PTR_EQ(test, cur, &entries[i]);
+		KUNIT_EXPECT_PTR_EQ(test, seplist_next(&entries[i]), &entries[(i+1) % (__NUM_ENTRIES + 1)]);
+		KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&entries[i]), &entries[i-1]);
+		i++;
+	}
+#undef __NUM_ENTRIES
+}
+
+static void seplist_test_seplist_add_middle(struct kunit *test)
+{
+	struct seplist_head a, b, c;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add(&a, &seplist);
+	seplist_add(&c, &seplist);
+	seplist_add(&b, &c);
+
+	/* should be [seplist] -> c -> b -> a */
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&seplist), &a);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&a), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&b), &c);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&c), &seplist);
+
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&seplist), &c);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&c), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&b), &a);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&a), &seplist);
+}
+
+
+static void seplist_test_seplist_add_tail_middle(struct kunit *test)
+{
+	struct seplist_head a, b, c;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add_tail(&c, &seplist);
+	seplist_add_tail(&a, &seplist);
+	seplist_add_tail(&b, &a);
+
+	/* should be [seplist] -> c -> b -> a */
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&seplist), &a);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&a), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&b), &c);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&c), &seplist);
+
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&seplist), &c);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&c), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&b), &a);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&a), &seplist);
+}
+
+
+static void seplist_test_seplist_del(struct kunit *test)
+{
+	struct seplist_head a, b;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add_tail(&a, &seplist);
+	seplist_add_tail(&b, &seplist);
+
+	/* before: [seplist] -> a -> b */
+	seplist_del(&a);
+
+	/* now: [seplist] -> b */
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&seplist), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&b), &seplist);
+}
+
+static void seplist_test_seplist_replace(struct kunit *test)
+{
+	struct seplist_head a_old, a_new, b;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add_tail(&a_old, &seplist);
+	seplist_add_tail(&b, &seplist);
+
+	/* before: [seplist] -> a_old -> b */
+	seplist_replace(&a_old, &a_new);
+
+	/* now: [seplist] -> a_new -> b */
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&seplist), &a_new);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&b), &a_new);
+}
+
+static void seplist_test_seplist_replace_init(struct kunit *test)
+{
+	struct seplist_head a_old, a_new, b;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add_tail(&a_old, &seplist);
+	seplist_add_tail(&b, &seplist);
+
+	/* before: [seplist] -> a_old -> b */
+	seplist_replace_init(&a_old, &a_new);
+
+	/* now: [seplist] -> a_new -> b */
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&seplist), &a_new);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&b), &a_new);
+
+	/* check a_old is empty (initialized) */
+	KUNIT_EXPECT_TRUE(test, seplist_empty(&a_old));
+}
+

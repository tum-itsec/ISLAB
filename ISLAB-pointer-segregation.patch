+static inline unsigned long
+ve_get_error_code(unsigned long exit_qualification)
+{
+	unsigned long error_code = X86_PF_PROT | X86_VE_EPT_VIO;
+
+	error_code |= (exit_qualification & EPT_VIOLATION_ACC_WRITE) ? X86_PF_WRITE : 0;
+	error_code |= (exit_qualification & EPT_VIOLATION_ACC_INSTR) ? X86_PF_INSTR : 0;
+	error_code |= (exit_qualification & EPT_VIOLATION_GVA_USER ) ? X86_PF_USER  : 0;
+
+	return error_code;
+}
+
+DEFINE_IDTENTRY(exc_virtualization_exception)
+{
+	int cpu;
+	struct ve_info_area *vea;
+	unsigned long error_code;
+	xenmem_access_t i_access;
+
+	if (regs->flags & X86_EFLAGS_IF)
+		local_irq_enable();
+
+	cpu = smp_processor_id();
+	vea = altp2m_vcpu_vea(cpu);
+	
+	/* Prevent VE trapping in VMM */
+	// vea->lock = 0;
+	
+	/* Get actual error code */
+	error_code = ve_get_error_code(vea->exit_qualification);
+
+	/* Print generic #VE information from virtualization exception info area */
+	pr_alert("#VE BUG: unable to access address in EPTP index %u: %016llx\n",
+		 vea->altp2m_id, vea->gva);
+
+	pr_alert("Access happened in instruction at RIP: %016lx", regs->ip);
+
+	dump_stack();
+
+#if defined(CONFIG_ISLAB_VTX)
+
+	altp2m_get_mem_access(metaview, vea->gpa >> PAGE_SHIFT, &i_access);
+	printk("ISLAB: Access permissions for %016llx in metaview: %u", vea->gpa, i_access);
+	altp2m_get_mem_access(dataview, vea->gpa >> PAGE_SHIFT, &i_access);
+	printk("ISLAB: Access permissions for %016llx in dataview: %u", vea->gpa, i_access);
+
+	altp2m_set_mem_access(vea->gva, vea->gpa >> PAGE_SHIFT, XENMEM_access_rwx);
+
+	goto resume;
+	
+#elif defined(CONFIG_SLUB_HARDENED_FREELIST_METASLAB)
+	
+	struct page *vea_gpa_page = pfn_to_page(vea->gpa >> PAGE_SHIFT);
+	printk("struct page -> compound_head %lx, page->flags %lx, page->type %lx", vea_gpa_page->compound_head, vea_gpa_page->flags, vea_gpa_page->page_type);
+	dump_page(vea_gpa_page, "VE");
+
+	unsigned long irq_count_before = irq_count();
+	printk("[VE] Nr interrupts: %d", irq_count_before);
+
+	{
+		extern int altp2m_get_mem_access(uint16_t, xen_pfn_t, xenmem_access_t *);
+		xenmem_access_t access_metaview, access_dataview;
+		altp2m_get_mem_access(metaview, vea->gpa >> PAGE_SHIFT, &access_metaview);
+		altp2m_get_mem_access(dataview, vea->gpa >> PAGE_SHIFT, &access_dataview);
+		printk("[VE] vea->gpa memaccess metaview %d dataview %d", access_metaview, access_dataview);
+		altp2m_get_mem_access(metaview, slow_virt_to_phys(vea->gva) >> PAGE_SHIFT, &access_metaview);
+		altp2m_get_mem_access(dataview, slow_virt_to_phys(vea->gva) >> PAGE_SHIFT, &access_dataview);
+		printk("[VE] slow_virt_to_phys(gva) memaccess metaview %d dataview %d", access_metaview, access_dataview);
+	}
+
+	printk("[VE] err_wr|err_instr|err_usr %d|%d|%d, exit_reason = %lx, exit_qualification = %lx, gva = %lx, gpa %lx, slow_virt_to_phys(gva) = %lx",
+			error_code & X86_PF_WRITE, error_code & X86_PF_INSTR, error_code & X86_PF_USER, vea->exit_reason, vea->exit_qualification, vea->gva, vea->gpa, slow_virt_to_phys(vea->gva));
+	
+	uint16_t altp2m_idx1, altp2m_idx2;
+	{
+	 	extern int altp2m_get_vcpu_p2m_idx(uint32_t, uint16_t *);
+		if (altp2m_get_vcpu_p2m_idx(0, &altp2m_idx1))
+			printk("Error");
+		if (altp2m_get_vcpu_p2m_idx(1, &altp2m_idx2))
+			printk("Error");
+		printk("[VE] cpu0 %d cpu1 %d vea %d", altp2m_idx1, altp2m_idx2, vea->altp2m_id);
+	}
+
+#endif /* CONFIG_SLUB_HARDENED_FREELIST_METASLAB */
+	
+	if (fault_in_kernel_space(vea->gva)) {
+		bad_area_nosemaphore(regs, error_code, vea->gva);
+	}
+
+	bad_area(regs, error_code, vea->gva);
+
+resume:
+
+	printk("ISLAB: Resume execution");
+
+	if (regs->flags & X86_EFLAGS_IF)
+		local_irq_enable();
+}

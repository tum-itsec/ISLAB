+		i++;
+	}
+
+	KUNIT_EXPECT_EQ(test, i, 5);
+
+	KUNIT_EXPECT_TRUE(test, seplist_empty(&seplist2));
+}
+
+static void seplist_test_seplist_splice_tail_init_rcu(struct kunit *test)
+{

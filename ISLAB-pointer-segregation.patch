+
+
+static void seplist_test_seplist_empty_rcu(struct kunit *test)
+{
+	/* This test doesn't check correctness under concurrent access */
+	struct seplist_head a;
+	SEPLIST_HEAD(seplist1);
+	SEPLIST_HEAD(seplist2);
+
+	seplist_add_tail_rcu(&a, &seplist1);
+
+	KUNIT_EXPECT_FALSE(test, seplist_empty(&seplist1));

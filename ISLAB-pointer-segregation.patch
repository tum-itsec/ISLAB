+
+
+static void seplist_test_seplist_is_singular_rcu(struct kunit *test)
+{
+	struct seplist_head a, b;
+	SEPLIST_HEAD(seplist);
+
+	/* [seplist] empty */
+	KUNIT_EXPECT_FALSE(test, seplist_is_singular(&seplist));
+
+	seplist_add_tail_rcu(&a, &seplist);
+
+	/* [seplist] -> a */
+	KUNIT_EXPECT_TRUE(test, seplist_is_singular(&seplist));
+
+	seplist_add_tail_rcu(&b, &seplist);
+
+	/* [seplist] -> a -> b */
+	KUNIT_EXPECT_FALSE(test, seplist_is_singular(&seplist));
+}
+
+static void seplist_test_seplist_splice_init_rcu(struct kunit *test)
+{
+	struct seplist_head entries[5], *cur;
+	int i = 0;
+	SEPLIST_HEAD(seplist1);
+	SEPLIST_HEAD(seplist2);
+
+	seplist_add_tail_rcu(&entries[0], &seplist1);
+	seplist_add_tail_rcu(&entries[1], &seplist1);
+	seplist_add_tail_rcu(&entries[2], &seplist2);
+	seplist_add_tail_rcu(&entries[3], &seplist2);
+	seplist_add_tail_rcu(&entries[4], &seplist1);

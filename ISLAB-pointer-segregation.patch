+#include <linux/rcuseplist.h>
+
+struct seplist_test_struct_rcu {
+	int data;
+	struct seplist_head seplist;
+};
+
+static void seplist_test_seplist_init_rcu(struct kunit *test)
+{
+	/* Test the different ways of initialising a seplist. */
+	struct seplist_head seplist1 = SEPLIST_HEAD_INIT(seplist1);
+	struct seplist_head seplist2;
+	SEPLIST_HEAD(seplist3);
+	struct seplist_head *seplist4;
+	struct seplist_head *seplist5;
+
+	INIT_SEPLIST_HEAD_RCU(&seplist2);
+
+	seplist4 = kzalloc(sizeof(*seplist4), GFP_KERNEL | __GFP_NOFAIL);
+	INIT_SEPLIST_HEAD_RCU(seplist4);
+
+	seplist5 = kmalloc(sizeof(*seplist5), GFP_KERNEL | __GFP_NOFAIL);
+	memset(seplist5, 0xFF, sizeof(*seplist5));
+	INIT_SEPLIST_HEAD_RCU(seplist5);
+
+	/* seplist_empty() checks both next and prev. */
+	KUNIT_EXPECT_TRUE(test, seplist_empty(&seplist1));
+	KUNIT_EXPECT_TRUE(test, seplist_empty(&seplist2));
+	KUNIT_EXPECT_TRUE(test, seplist_empty(&seplist3));
+	KUNIT_EXPECT_TRUE(test, seplist_empty(seplist4));
+	KUNIT_EXPECT_TRUE(test, seplist_empty(seplist5));
+
+	kfree(seplist4);
+	kfree(seplist5);
+}
+
+static void seplist_test_seplist_add_rcu(struct kunit *test)
+{
+	struct seplist_head a, b;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add_rcu(&a, &seplist);
+	seplist_add_rcu(&b, &seplist);
+
+	/* should be [seplist] -> b -> a */
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&seplist), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&b), &seplist);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&b), &a);
+}
+
+
+static void seplist_test_seplist_add_tail_rcu(struct kunit *test)
+{
+	struct seplist_head a, b;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add_tail_rcu(&a, &seplist);
+	seplist_add_tail_rcu(&b, &seplist);
+
+	/* should be [seplist] -> a -> b */
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&seplist), &a);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&seplist), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&a), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&a), &seplist);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&b), &seplist);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&b), &a);
+}
+
+
+
+static void seplist_test_seplist_add_many_rcu(struct kunit *test)
+{
+#define __NUM_ENTRIES (SEPLIST_CHUNK_ENTRIES*2)
+	struct seplist_head entries[__NUM_ENTRIES+1], *cur;
+	int i = 0;
+	INIT_SEPLIST_HEAD(&entries[0]);
+
+	for(i = __NUM_ENTRIES-1; i >= 0; i--)
+		seplist_add_rcu(&entries[i+1], &entries[0]);
+
+	i = 1;
+	seplist_for_each(cur, &entries[0]) {
+		KUNIT_EXPECT_PTR_EQ(test, cur, &entries[i]);
+		KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&entries[i]), &entries[(i+1) % (__NUM_ENTRIES + 1)]);
+		KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&entries[i]), &entries[i-1]);
+		i++;
+	}
+#undef __NUM_ENTRIES
+}
+
+static void seplist_test_seplist_add_tail_many_rcu(struct kunit *test)
+{
+#define __NUM_ENTRIES 3
+	struct seplist_head entries[__NUM_ENTRIES+1], *cur;
+	int i = 0;
+	INIT_SEPLIST_HEAD(&entries[0]);
+
+	for(i = 0; i < __NUM_ENTRIES; i++)
+		seplist_add_tail_rcu(&entries[i+1], &entries[0]);
+
+	i = 1;
+	seplist_for_each(cur, &entries[0]) {
+		KUNIT_EXPECT_PTR_EQ(test, cur, &entries[i]);
+		KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&entries[i]), &entries[(i+1) % (__NUM_ENTRIES + 1)]);
+		KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&entries[i]), &entries[i-1]);
+		i++;
+	}
+#undef __NUM_ENTRIES
+}
+
+static void seplist_test_seplist_add_middle_rcu(struct kunit *test)
+{
+	struct seplist_head a, b, c;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add_rcu(&a, &seplist);
+	seplist_add_rcu(&c, &seplist);
+	printk("foo1\n");
+	seplist_add_rcu(&b, &c);
+	printk("foo2\n");
+
+	/* should be [seplist] -> c -> b -> a */
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&seplist), &a);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&a), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&b), &c);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&c), &seplist);
+
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&seplist), &c);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&c), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&b), &a);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&a), &seplist);
+}
+
+
+static void seplist_test_seplist_add_tail_middle_rcu(struct kunit *test)
+{
+	struct seplist_head a, b, c;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add_tail_rcu(&c, &seplist);
+	seplist_add_tail_rcu(&a, &seplist);
+	seplist_add_tail_rcu(&b, &a);
+
+	/* should be [seplist] -> c -> b -> a */
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&seplist), &a);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&a), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&b), &c);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&c), &seplist);
+
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&seplist), &c);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&c), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&b), &a);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&a), &seplist);
+}
+
+
+static void seplist_test_seplist_del_rcu(struct kunit *test)
+{
+	struct seplist_head a, b;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add_tail_rcu(&a, &seplist);
+	seplist_add_tail_rcu(&b, &seplist);
+
+	/* before: [seplist] -> a -> b */
+	seplist_del_rcu(&a);
+
+	/* now: [seplist] -> b */
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&seplist), &b);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&b), &seplist);
+}
+
+static void seplist_test_seplist_replace_rcu(struct kunit *test)
+{
+	struct seplist_head a_old, a_new, b;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add_tail_rcu(&a_old, &seplist);
+	seplist_add_tail_rcu(&b, &seplist);
+
+	/* before: [seplist] -> a_old -> b */
+	seplist_replace_rcu(&a_old, &a_new);
+
+	/* now: [seplist] -> a_new -> b */
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next_rcu(&seplist), &a_new);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev_rcu(&b), &a_new);
+
+	rcu_barrier();
+}
+
+static void seplist_test_seplist_is_first_rcu(struct kunit *test)
+{
+	struct seplist_head a, b;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add_tail_rcu(&a, &seplist);
+	seplist_add_tail_rcu(&b, &seplist);
+
+	KUNIT_EXPECT_TRUE(test, seplist_is_first(&a, &seplist));
+	KUNIT_EXPECT_FALSE(test, seplist_is_first(&b, &seplist));
+}
+
+static void seplist_test_seplist_is_last_rcu(struct kunit *test)
+{
+	struct seplist_head a, b;
+	SEPLIST_HEAD(seplist);
+
+	seplist_add_tail_rcu(&a, &seplist);
+	seplist_add_tail_rcu(&b, &seplist);
+
+	KUNIT_EXPECT_FALSE(test, seplist_is_last(&a, &seplist));
+	KUNIT_EXPECT_TRUE(test, seplist_is_last(&b, &seplist));

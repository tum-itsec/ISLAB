+	seplist_add_tail(&entries[0], &seplist);
+	seplist_add_tail(&entries[1], &seplist);
+	seplist_add_tail(&entries[2], &seplist);
+
+	seplist_for_each(cur, &seplist) {
+		KUNIT_EXPECT_PTR_EQ(test, cur, &entries[i]);
+		i++;
+	}
+
+	KUNIT_EXPECT_EQ(test, i, 3);
+}
+
+
+
+static void seplist_test_seplist_for_each_entry_rcu(struct kunit *test)
+{
+	struct seplist_test_struct_rcu entries[5], *cur;
+	SEPLIST_HEAD(seplist);
+	SEPLIST_HEAD(seplist_empty);
+	int i = 0;
+
+	for (i = 0; i < 5; ++i) {
+		entries[i].data = i;
+		seplist_add_tail(&entries[i].seplist, &seplist);
+	}
+
+	i = 0;

+/**
+ * seplist_del_init_careful - deletes entry from seplist and reinitialize it.
+ * @entry: the element to delete from the seplist.
+ *
+ * This is the same as seplist_del_init(), except designed to be used
+ * together with seplist_empty_careful() in a way to guarantee ordering
+ * of other memory operations.
+ *
+ * Any memory operations done before a seplist_del_init_careful() are
+ * guaranteed to be visible after a seplist_empty_careful() test.
+ */
+static inline void seplist_del_init_careful(struct seplist_head *entry)
+{
+	struct seplist_chunk *cur_chunk;
+	struct seplist_head **next;
+	void** meta;
+	int empty;
+
+	if(!entry->meta)
+		return;
+
+	smapdisable();
+
+	seplist_check_integrity(entry);
+
+	meta = entry->meta;
+	cur_chunk = seplist_meta_to_chunk(meta);
+
+	*meta = NULL;
+
+	next = seplist_next_sepentry(meta);
+
+	empty = sepchunk_empty(cur_chunk);
+	if(empty)
+		__seplist_unlink_chunk(cur_chunk);
+
+	smp_store_release(&entry->meta, NULL);
+
+	if(empty)
+		__seplist_del_chunk(cur_chunk);
+
+
+	if(seplist_next_sepentry((void **)next) == next)
+	{
+		cur_chunk = seplist_meta_to_chunk(next);
+		smp_store_release(&(*next)->meta, NULL);
+		__seplist_free_chunk(cur_chunk);
+	}
+
+	smapenable();
+}

+static inline void seplist_check_integrity(const struct seplist_head *list)
+{
+	void **listmeta;
+	listmeta = READ_ONCE(list->meta);
+	if(listmeta) {
+		if (kmem_seplist_chunk_cache != NULL)
+			BUG_ON(seplist_get_kmemcache(listmeta) != kmem_seplist_chunk_cache);

+/**
+ * seplist_cleanup - cleanup a seplist about to be deleted
+ * @head: head of the seplist
+ *
+ * This function is intended to be used before a head is freed
+ * and potentially not all elements have been deleted from it
+ */
+static inline void seplist_cleanup(struct seplist_head *seplist) {
+	struct seplist_chunk *head, *cur, *next;
+	void** meta = seplist->meta;
+
+	if(!meta)
+		return;
+
+	head = cur = seplist_meta_to_chunk(meta);
+	smapdisable();
+	do {
+		next = cur->next;
+		__seplist_free_chunk(cur);
+		cur = next;
+	}
+	while(cur != head);
+	smapenable();
+}

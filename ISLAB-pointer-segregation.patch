+// #define ALTP2M_SANITY_CHECK
+
+void altp2m_init_and_test(void)
+{
+	int rc;
+#ifdef ALTP2M_SANITY_CHECK
+	unsigned int cpu = smp_processor_id();
+	uint16_t view1 = -1, view2 = -1;
+	unsigned long *addr;
+	uint16_t current_view = -1;
+	xenmem_access_t access = 0;
+	bool sve = true;
+	struct page *page;
+#endif
+
+	rc = altp2m_set_domain_state(true);
+	if (rc)
+		printk("Error setting domain state");
+
+	rc = altp2m_alloc_vcpus();
+	if (rc)
+		printk("Error allocing vcpus");
+
+	altp2m_init_vcpu(NULL);
+
+#ifdef ALTP2M_SANITY_CHECK
+	rc = altp2m_create_view(XENMEM_access_default, &view1);
+	if (rc)
+		printk("Error creating view");
+
+	rc = altp2m_create_view(XENMEM_access_default, &view2);
+	if (rc)
+		printk("Error creating view");
+
+	printk("view1 = %d, view2 = %d", view1, view2);
+	
+	/*
+	 * We cannot use kmalloc() for the sanity check because kmem_cache_init()
+	 * is yet to be called! We replace it with get_zeroed_page() since it was
+	 * already initialized.
+	 *
+	 * Leave this warning for the future so that no one will keep looking for
+	 * the bug that causes the sanity check to fail :)
+	 *
+	 * addr = kmalloc(512, GFP_KERNEL);
+	 */
+	page = alloc_page(GFP_KERNEL | __GFP_ZERO);
+	addr = page_address(page);
+	
+	if (!addr) {
+		printk("No memory? Already? Panic ...");	
+		BUG_ON(true);
+	}
+
+	*addr = '\x42';
+	printk("*addr = %lx", *addr);
+
+	rc = altp2m_set_mem_access(view1, virt_to_pfn(addr), XENMEM_access_rwx);
+	if (rc)
+		printk("Error setting mem access in the view1: %d", -rc);
+
+	rc = altp2m_set_mem_access(view2, virt_to_pfn(addr), XENMEM_access_n);
+	if (rc)
+		printk("Error setting mem access in the view2: %d", -rc);
+
+	rc = altp2m_get_suppress_ve(view2, virt_to_pfn(addr), &sve);
+	if (rc)
+		printk("Error getting suppress ve: %d", -rc);
+	printk("SVE view2 = %d", sve);
+
+	rc = altp2m_get_suppress_ve(view1, virt_to_pfn(addr), &sve);
+	if (rc)
+		printk("Error getting suppress ve: %d", -rc);
+	printk("SVE view1 = %d", sve);
+
+	rc = altp2m_set_suppress_ve(view2, virt_to_pfn(addr), false);
+	if (rc)
+		printk("Error setting suppress ve: %d", -rc);
+
+	rc = altp2m_set_suppress_ve(view1, virt_to_pfn(addr), false);
+	if (rc)
+		printk("Error setting suppress ve: %d", -rc);
+
+	rc = altp2m_switch_to_view(view2);
+	if (rc)
+		printk("Error switching to the view2: %d", -rc);
+
+	rc = altp2m_switch_to_view(view1);
+	if (rc)
+		printk("Error switching to the view1: %d", -rc);
+
+	rc = altp2m_get_vcpu_p2m_idx(cpu, &current_view);
+	if (rc)
+		printk("Error getting view idx: %d", -rc);
+	printk("current_view = %d", current_view);
+
+	rc = altp2m_switch_to_view(view2);
+	if (rc)
+		printk("Error switching to the view2: %d", -rc);
+
+	rc = altp2m_get_vcpu_p2m_idx(cpu, &current_view);
+	if (rc)
+		printk("Error getting view idx: %d", -rc);
+	printk("current_view = %d", current_view);
+
+	rc = vmfunc(view1);
+	if (rc)
+		printk("Error vmfunc: %d", -rc);
+
+	rc = altp2m_get_vcpu_p2m_idx(cpu, &current_view);
+	if (rc)
+		printk("Error getting view idx: %d", -rc);
+	printk("current_view = %d", current_view);
+
+	// We shouldn't crash here
+	*addr = '\x42';
+	printk("*addr = %lx", *addr);
+
+	rc = altp2m_get_mem_access(view2, virt_to_pfn(addr), &access);
+	if (rc)
+		printk("Error getting mem access: %d", -rc);
+	printk("view2 mem access = %d", access);
+
+	rc = altp2m_get_mem_access(view1, virt_to_pfn(addr), &access);
+	if (rc)
+		printk("Error getting mem access: %d", -rc);
+	printk("view1 mem access = %d", access);
+
+	printk("addr gfn == %lx", (unsigned long)(__pa(addr) >> 12));
+	printk("addr virt_to_pfn == %lx", (unsigned long)(virt_to_pfn(addr)));
+	
+	rc = vmfunc(view2);
+	if (rc)
+		printk("Error vmfunc: %d", -rc);
+
+#if 0	// Enable if you want to test the VE handler in action and see the kernel
+	// gracefully crashing
+	*addr = '\xff';
+	printk("*addr = %x", *addr);
+	if (*addr == '\xff')
+		printk("This should not happen: \\xff");
+#endif
+
+	rc = vmfunc(0);
+	if (rc)
+		printk("Error vmfunc: %d", -rc);
+
+	if (addr) {
+		
+		/*
+		 * Do not use kfree as kmem_cache_init() has not been called yet!
+		 * We replace the function to free the page allocated above.
+		 *
+		 * kfree(addr);
+		 */
+		free_page((unsigned long)addr);
+	}
+
+#endif
+}

diff --git a/include/linux/seplist.h b/include/linux/seplist.h
new file mode 100644
index 000000000000..a0fb8ee6fb96
--- /dev/null
+++ b/include/linux/seplist.h
@@ -0,0 +1,2256 @@
+/* SPDX-License-Identifier: GPL-2.0 */
+#ifndef _LINUX_SEPLIST_H
+#define _LINUX_SEPLIST_H
+
+#include <linux/types.h>
+#include <asm/bug.h>
+#include <linux/stddef.h>
+#include <linux/poison.h>
+#include <linux/const.h>
+#include <linux/kernel.h>
+#include <linux/string.h>
+#include <linux/atomic.h>
+#include <asm/page.h>
+
+/*
+ * Drop-in doubly linked list implementation.
+ *
+ * Replaces the functionality of a doubly linked list by separating the metadata
+ * allows for RCU and non-RCU variants and supports the same API.
+ *
+ * Linkage information is put into seplist_chunks which contain several backwards
+ * ptrs to fill cacheline info
+ *
+ */
+
+#ifdef CONFIG_SEPLIST_MAGAZINE
+#define SEPLIST_CHUNK_CACHELINES                         1
+#define SEPLIST_CHUNK_ENTRIES                            (((L1_CACHE_BYTES * SEPLIST_CHUNK_CACHELINES) - (2 * sizeof(void*))) / sizeof(void*))
+#define SEPLIST_CHUNK_SIZE                               (sizeof(struct seplist_chunk))
+#define seplist_meta_to_chunk(ptr)                       ((struct seplist_chunk*)((uintptr_t)ptr & ~(uintptr_t)(SEPLIST_CHUNK_SIZE-1)))
+#define seplist_meta_to_chunkidx(ptr)                    (((uintptr_t)ptr & (SEPLIST_CHUNK_SIZE-1)) / sizeof(void*))
+
+#define seplist_rcu_delptr(ptr)                          ((void*)((uintptr_t)ptr | 1))
+#define seplist_rcu_getptr(ptr)                          ((struct seplist_head*)((uintptr_t)ptr & ~(uintptr_t)1))
+
+#define seplist_entry_marked_delete(ptrval)              ((uintptr_t)ptrval & 1)
+#define seplist_entry_has_entry(ptrval)                  (ptrval && !seplist_entry_marked_delete(ptrval))
+
+
+
+struct seplist_chunk {
+	void *obj[SEPLIST_CHUNK_ENTRIES];
+	struct seplist_chunk *next;
+	struct seplist_chunk *prev;
+};
+
+#elif defined(CONFIG_SEPLIST_SINGLE)
+struct seplist_chunk {
+	void *obj;
+	struct seplist_chunk *next;
+	struct seplist_chunk *prev;
+};
+
+#define SEPLIST_CHUNK_ENTRIES                            1
+#define seplist_meta_to_chunk(ptr)                       ((struct seplist_chunk*)ptr)
+#define seplist_rcu_delptr(ptr)                          (ptr)
+#define seplist_rcu_getptr(ptr)                          (ptr)
+
+#endif
+
+#ifdef CONFIG_SEPLIST_KSMAP
+#include <linux/ksmap.h>
+
+#define smapenable() ksmap_enable_smap()
+#define smapdisable() ksmap_disable_smap()
+
+#else
+#define smapenable() do { } while (0)
+#define smapdisable() do { } while (0)
+
+#endif
+
+extern void __init create_seplist_chunk_cache(slab_flags_t flags);
+extern struct kmem_cache *kmem_seplist_chunk_cache;
+extern struct kmem_cache *kmem_seplist_rcustruct_cache;
+
+#define SEPLIST_HEAD_INIT(name) { NULL }
+#define SEPLIST_ENTRY_INIT_ZERO  { NULL }
+
+#define SEPLIST_HEAD(name) \
+	struct seplist_head name = SEPLIST_HEAD_INIT(name)
+
+/**
+ * INIT_LIST_HEAD - Initialize a list_head structure

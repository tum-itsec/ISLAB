+static inline void *pop_freepointer(struct kmem_cache *s, struct page *page, void **freelist)
+{
+	void *obj;
+	struct kmem_freelist_node* list_node;
+	void* freelist_val = *freelist;
+
+	uint16_t offset = kmem_freelist_get_offset(freelist_val);
+	list_node = kmem_freelist_get_node(freelist_val);
+	obj = slab_obj_by_index(offset, s, page);
+	*freelist = list_node->next;
+
+	_free_freelist_node(list_node, page, s, obj, freelist);
+
+	return obj;

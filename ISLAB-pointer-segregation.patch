+/**
+ * We need the entry head as splitting might move the entry to another position
+ */
+static inline void __seplist_cut_position(struct seplist_head *seplist_h,
+		void **head, struct seplist_head *_entry)
+{
+	struct seplist_chunk *headchunk, *seplist, *new_first, *new_last;
+	void **entry;
+
+	/* Assume uninitialized seplist */
+	{
+		seplist = new_seplist_chunk_init();
+		seplist->obj[SEPLIST_CHUNK_ENTRIES-1] = &seplist_h->meta;
+		WRITE_ONCE(seplist_h->meta, &seplist->obj[SEPLIST_CHUNK_ENTRIES-1]);
+	}
+
+
+	if(!seplist_is_head_last_in_chunk(head))
+		seplist_split_chunk(head);
+
+
+	entry = _entry->meta;
+
+	if(!seplist_is_head_last_in_chunk(entry))
+		seplist_split_chunk(entry);
+
+	headchunk = seplist_meta_to_chunk(head);
+	new_first = headchunk->next;
+	new_last = seplist_meta_to_chunk(entry);
+
+
+	seplist->next = new_first;
+	seplist->prev = new_last;
+
+	headchunk->next = new_last->next;
+	headchunk->next->prev = headchunk;
+
+	new_last->next = seplist;
+	new_first->prev = seplist;
+
+	if((void **)seplist_next_sepentry(head) == head) {
+		(*(struct seplist_head **)head)->meta = NULL;
+		__seplist_free_chunk(headchunk);
+	}
+}

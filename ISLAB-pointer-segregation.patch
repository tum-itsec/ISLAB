+static inline void __seplist_del_deferred_empty_ptr(void *meta)
+{
+	unsigned long flags;
+	struct seplist_chunk *cur_chunk = seplist_meta_to_chunk(meta);
+
+	/* Make sure no other CPU is deleting a ptr and checking for emptiness */
+	spin_lock_irqsave(&seplist_rcu_deferred_del_spinlock, flags);
+	WRITE_ONCE(*(void**)meta, NULL);
+
+	/* If sepchunk_empty, this means that the chunk must already
+	 * be unlinked (as it was unused before), but still had pending
+	 * empty_ptr calls. Now that it's empty, all empty_ptr calls went
+	 * through so we can free it. Make sure to unlock before the free */
+	if (sepchunk_empty(cur_chunk)) {
+		spin_unlock_irqrestore(&seplist_rcu_deferred_del_spinlock, flags);
+		__seplist_free_chunk(cur_chunk);
+	} else {
+		spin_unlock_irqrestore(&seplist_rcu_deferred_del_spinlock, flags);
+	}
+}
+
+static inline void __wrapped_seplist_del_deferred_empty_ptr(void *meta) {
+	smapdisable();
+	__seplist_del_deferred_empty_ptr(meta);
+	smapenable();
+}

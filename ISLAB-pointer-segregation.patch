+static inline void __seplist_add_chunk(struct seplist_chunk* new,
+		struct seplist_chunk* prev,
+		struct seplist_chunk* next)
+{
+	new->next = next;
+	new->prev = prev;
+
+
+	prev->next = new;
+	next->prev = new;
+}
+
+static inline void __seplist_unlink_chunk(struct seplist_chunk* chunk)
+{
+	struct seplist_chunk *prev = chunk->prev;
+	struct seplist_chunk *next = chunk->next;
+	next->prev = prev;
+	WRITE_ONCE(prev->next, next);
+}
+
+static inline void __seplist_del_chunk(struct seplist_chunk* chunk)
+{
+	__seplist_unlink_chunk(chunk);
+	__seplist_free_chunk(chunk);
+}

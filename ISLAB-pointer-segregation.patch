+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&testi.elts[1]->list), &testi.elts[0]->list);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_prev(&testi.elts[1]->list), &head);
+	KUNIT_EXPECT_PTR_EQ(test, seplist_next(&testi.elts[0]->list), &head);

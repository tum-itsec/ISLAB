+static inline int seplist_is_chunk_empty(struct seplist_chunk* chunk)
+{
+	int i;
+	for(i = 0; i < SEPLIST_CHUNK_ENTRIES; i++)
+		if(seplist_entry_has_entry(chunk->obj[i]))
+			return 0;
+	return 1;
+}
+
+static inline int seplist_has_empty_chunk(void **meta)
+{
+	struct seplist_chunk *start = seplist_meta_to_chunk(meta), *cur = start;
+
+	do
+	{
+		if(seplist_is_chunk_empty(cur))
+			return true;
+		cur = cur->next;
+	} while (cur != start);
+	return false;
+}
+
+static inline void** seplist_split_chunk(void **chunk_pos)
+{
+	size_t i, j, cidx;
+	struct seplist_chunk *cur_chunk, *new_chunk;
+	void **r;
+	cur_chunk = seplist_meta_to_chunk(chunk_pos);
+	new_chunk = new_seplist_chunk();
+	cidx = seplist_meta_to_chunkidx(chunk_pos);
+
+	for(i = cidx + 1, j = 0; i < SEPLIST_CHUNK_ENTRIES; i++)
+	{
+		r = READ_ONCE(cur_chunk->obj[i]);
+		if(seplist_entry_has_entry(r))
+		{
+			new_chunk->obj[j] = r;
+			*r = &new_chunk->obj[j];
+			cur_chunk->obj[i] = NULL;
+			j++;
+			seplist_check_integrity((struct seplist_head *)r);
+		}
+	}
+
+	__seplist_add_chunk(new_chunk, cur_chunk, cur_chunk->next);
+
+	if(cidx != SEPLIST_CHUNK_ENTRIES-1)
+		return (chunk_pos + 1);
+	return &new_chunk->obj[0];
+}
+
+static inline void *seplist_get_last_freespot_in_chunk(struct seplist_chunk *chunk, ssize_t start_idx)
+{
+	ssize_t i = start_idx;
+	void *ret;
+	if(start_idx >= SEPLIST_CHUNK_ENTRIES || seplist_entry_has_entry(chunk->obj[start_idx]))
+		goto out;
+	/* Take last empty spot */
+	while(i < SEPLIST_CHUNK_ENTRIES && !seplist_entry_has_entry(chunk->obj[i])) i++;
+	for(--i; i >= start_idx && seplist_entry_marked_delete(chunk->obj[i]); --i);
+	if(i < start_idx) goto out;
+	ret = &chunk->obj[i];
+	return ret;
+out:
+	return NULL;
+}
+
+static inline void *seplist_get_first_freespot_in_chunk(struct seplist_chunk *chunk, ssize_t start_idx)
+{
+	ssize_t i = start_idx;
+	void *ret;
+	if(start_idx < 0 || seplist_entry_has_entry(chunk->obj[start_idx]))
+		goto out;
+	/* Take last empty spot */
+	while(i >= 0 && !seplist_entry_has_entry(chunk->obj[i])) i--;
+	for(++i; i <= start_idx && seplist_entry_marked_delete(chunk->obj[i]); ++i);
+	if(i > start_idx) goto out;
+	ret = &chunk->obj[i];
+	return ret;
+out:
+	return NULL;
+}
+
+static inline void *seplist_get_next_used_spot_in_chunk(struct seplist_chunk* chunk, ssize_t start_idx)
+{
+	ssize_t i;
+	for(i = start_idx; i < SEPLIST_CHUNK_ENTRIES; i++)
+		if(seplist_entry_has_entry(chunk->obj[i]))
+			return &chunk->obj[i];
+	return NULL;
+}
+
+
+static inline void *seplist_get_prev_used_spot_in_chunk(struct seplist_chunk* chunk, ssize_t start_idx)
+{
+	ssize_t i;
+	for(i = start_idx; i >= 0; i--)
+		if(seplist_entry_has_entry(chunk->obj[i]))
+			return &chunk->obj[i];
+
+	return NULL;
+}
+
+static inline struct seplist_head *seplist_get_next_obj_in_chunk(struct seplist_chunk* chunk, ssize_t start_idx)
+{
+	ssize_t i;
+	void *r;
+	
+	for(i = start_idx; i < SEPLIST_CHUNK_ENTRIES; i++)
+	{
+		r = READ_ONCE(chunk->obj[i]);
+		if (seplist_entry_has_entry(r))
+			return r;
+	}
+
+	return NULL;
+}
+
+
+static inline void *seplist_get_prev_obj_in_chunk(struct seplist_chunk* chunk, ssize_t start_idx)
+{
+	ssize_t i;
+	void *r;
+	for (i = start_idx; i >= 0; i--) {
+		r = READ_ONCE(chunk->obj[i]);
+		if (seplist_entry_has_entry(r))
+			return r;
+		}
+
+	return NULL;
+}

+static inline void seplist_del(struct seplist_head *entry)
+{
+	struct seplist_chunk* cur_chunk;
+	struct seplist_chunk *next;
+
+	cur_chunk = (struct seplist_chunk *)entry->meta;
+
+	if(!cur_chunk)
+		return;
+
+	smapdisable();
+
+	seplist_check_integrity(entry);
+	next = cur_chunk->next;
+	__seplist_del_chunk(cur_chunk);
+
+
+	/* Delete empty heads to prevent memory leaks*/
+	if(next->next == next)
+	{
+		*(void**)next->obj = NULL;
+		__seplist_free_chunk(next);
+	}
+
+	smapenable();
+}

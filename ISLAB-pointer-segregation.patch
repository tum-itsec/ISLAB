+/* Pushes an element onto a freelist
+ * this function assumes irq is disabled and no concurrent access can occur */
+static inline void push_freepointer(struct kmem_cache *s, struct page *slab, void* obj, void **freelist)
+{
+	void *next_freelist;
+	uint16_t offset;
+	void *cur = *freelist;
+	offset = obj_to_index(s, slab, obj);
+
+
+	/* If no freelist node was allocated yet, do so now */
+	if(!cur || kmem_freelist_full(cur)) {
+		struct kmem_freelist_node* freelist_new;
+		freelist_new = allocate_freelist_node(slab, s, obj);
+		memset(freelist_new->freeoffsets, 0xff, sizeof(freelist_new->freeoffsets));
+		freelist_new->next = cur;
+		next_freelist = kmem_freelist_node_to_ptr(freelist_new, 0);
+	}
+	else
+		next_freelist = kmem_freelist_increased_fill(cur);
+
+	*(uint16_t*)next_freelist = offset;
+	*freelist = next_freelist;
+}
